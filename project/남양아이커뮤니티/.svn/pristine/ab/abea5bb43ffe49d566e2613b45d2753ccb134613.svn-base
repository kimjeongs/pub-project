	<!-- Initialize Swiper -->
	<script>
		var options = {},
		slideLength = $('.main-swiper .swiper-slide').length,
		twinItem = $('.sub-swiper .swiper-slide'),
		twinItemTar = $('.sub-swiper .swiper-wrapper');
		if (slideLength == 1) {
			options = {
				autoplay: false,
				loop: false,
				simulateTouch: false
			}
			$('.swiper-button-prev, .swiper-button-next').hide();
		} else {
			options = {
				navigation: {
					nextEl: '.swiper-button-next',
					prevEl: '.swiper-button-prev',
				},
				autoplay: {
					delay: 4000,
				},
				centeredSlides: true,
				slidesPerView: 'auto',
				simulateTouch: false,
				speed: 500,
				loop: true,
				spaceBetween: 30
			}
		}
		var swiper1 = new Swiper('.main-swiper', options);

		twinItem.clone().appendTo(twinItemTar);
		var swiper2 = new Swiper('.sub-swiper', {
			navigation: {
				nextEl: '.swiper-button-next',
				prevEl: '.swiper-button-prev',
			},
			slidesPerView: 10,
			simulateTouch: false,
			speed: 500,
			loop: true,
			spaceBetween: 20
		});

		$('.main-swiper').on('mouseenter mouseleave', function (e) {
			if (e.type == 'mouseenter') {
				swiper1.autoplay.stop();
			} else {
				swiper1.autoplay.start();
			}
		});
	</script>
