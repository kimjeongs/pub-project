    <script type="text/javascript">
        if (window.location.href.indexOf('#') != -1) {
            window.location.href = window.location.href.split('#')[0];
        }

        var isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ? true : false;
        $(document).ready(function () {
            var swiper = new Swiper('.banner', {
                slidesPerView: 2,
            });
            var swiper = new Swiper('.m-banner', {
                pagination: {
                    el: '.swiper-pagination',
                },
            });
            var swiper = new Swiper('.event-slide', {
                on: {
                    slideNextTransitionEnd: function () {
                        var idx = $(this.$el).find('.swiper-slide-active').index();
                        $('.ctab li').eq(idx).find('a').trigger('click');
                    },
                    slidePrevTransitionEnd: function () {
                        var idx = $(this.$el).find('.swiper-slide-active').index();
                        $('.ctab li').eq(idx).find('a').trigger('click');
                    }
                },
                navigation: {
                    nextEl: '.event-slide .swiper-button-next',
                    prevEl: '.event-slide .swiper-button-prev',
                },
                pagination: {
                    el: '.swiper-pagination',
                    clickable: true
                }
            });

            $('.ctab li a').on('click', function (e) {
                e.preventDefault();
                var idx = $(this).parent().index();
                var crItem = $('.event-slide').find('.swiper-pagination > span')
                $('.ctab li a').parents().removeClass('on');
                $(this).parents().addClass('on');
                crItem.eq(idx).trigger('click')
            })

            var visualIdx = 0;
            $('#visualSlide .controls .slide-next').click(function () {
                var idx = $('#visualSlide li.on').index();

                var len = $('#visualSlide li').length - 1;

                if (idx + 1 > len) {
                    visualIdx = 0;
                } else {
                    visualIdx++;
                }
                $('#visualSlide li').eq(visualIdx).addClass('on').siblings().removeClass('on')
            });
            $('#visualSlide .controls .slide-prev').click(function () {
                var idx = $('#visualSlide li.on').index();
                var len = $('#visualSlide li').length - 1;
                if (idx - 1 < 0) {
                    visualIdx = len;
                } else {
                    visualIdx--;
                }
                $('#visualSlide li').eq(visualIdx).addClass('on').siblings().removeClass('on')
            });



            var swiper = new Swiper('.hash-slide', {
                navigation: {
                    nextEl: '.hash-slide .swiper-button-next',
                    prevEl: '.hash-slide .swiper-button-prev',
                },
            });
        });

        $('.m-menu').bind('click', function () {
            var me = $(this);
            var scrollTop = $(window).scrollTop();

            if (me.is('.open')) {
                if (scrollTop >= 1) {
                    setTimeout(function () {
                        $('#header').addClass('black');
                    }, 10)
                }
            }
        })

        if (isMobile) {
            $(window).scroll(function () {
                var scrollTop = $(this).scrollTop();
                if (scrollTop >= 1) {
                    $('#header').addClass('black');
                } else {
                    $('#header').removeClass('black');
                }
            })
        }
    </script>
</body>
</html>
