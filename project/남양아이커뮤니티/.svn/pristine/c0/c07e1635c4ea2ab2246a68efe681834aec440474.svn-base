/* left-cont */
.left-content {position:relative;float:left;width:840px;padding-bottom:90px;}
.left-content .c-title {font-size:32px;font-weight:700;color:#000;line-height:50px;letter-spacing:-1.5px;}

/* content navi */
.cont-navi {overflow:hidden;width:100%;}
.cont-navi li {float:left;}
.cont-navi li a,
.cont-navi li span {position:relative;margin-left:10px;padding-left:14px;font-size:13px;color:#aaa;line-height:74px;letter-spacing:0;}
.cont-navi li .nav-home {padding-left:17px;}
.cont-navi li .nav-home:before {content:'';position:absolute;top:50%;left:0;width:12px;height:11px;margin-top:-6px;background:url(../images/content/icon/ico_home.png) no-repeat center center;}
.cont-navi li .nav-sub:before {content:'';position:absolute;top:50%;left:0;width:5px;height:9px;margin-top:-5px;background:url(../images/content/icon/ico_navi_arrow.png) no-repeat center center;}
.cont-navi li span.nav-sub {color:#000;}

/* FOCOS_01_10 */
.cont-search-box {position:relative;width:460px;margin:0 auto;border-bottom:3px solid #000;}
.cont-search-box input {width:100%;height:48px;padding:13px 42px 13px 10px;border:0;font-size:15px;color:#000;line-height:normal;}
.cont-search-box input::-webkit-input-placeholder {color:#000;}
.cont-search-box input::placeholder {color:#000;}
.cont-search-box input:-ms-input-placeholder {color:#000;}
.cont-search-box button {position:absolute;top:50%;right:0;width:43px;height:45px;margin-top:-23px;}
.cont-search-box + .hor-tab-box {margin-top:41px;}

/* content list box */
.cont-list-box {margin-top:50px;}
.cont-search-result {margin:-4px 0 11px;font-size:16px;color:#000;line-height:24px;}
.cont-search-result.type2 {margin-bottom:-20px;margin-top:40px;}
.cont-search-result strong {color:#ec1f2f;letter-spacing:-1.5px;}
.cont-list-item {border-top:2px solid #000;}
.cont-list-item li {display:table;table-layout:fixed;width:100%;min-height:70px;border-bottom:1px solid #ddd;}
.cont-list-item li.notice,
.cont-list-item li.best {background:#f6f6f6;}
.cont-list-item li.notice.v-type .list-text {padding:10px 25px;}
.cont-list-item li.notice.v-type .list-text .best {display:table-cell;vertical-align:middle;}
.cont-list-item li.notice.v-type .list-text a {display:table-cell;vertical-align:middle;word-break:break-all;}
.cont-list-item li > span {display:table-cell;width:7%;font-size:15px;color:#000;line-height:30px;text-align:right;vertical-align:middle;}
.cont-list-item li .list-text {display:table-cell;width:77%;height:69px;padding:0 30px;vertical-align:middle;}
.cont-list-item li .list-text a {position:relative;font-size:15px;color:#444;line-height:22px;}
.cont-list-item li .list-text .best + a {margin-left:5px;vertical-align:middle;} 
.cont-list-item li .accordion-box .list-text a {color:#000;}
.cont-list-item li.focus .list-text a,
.cont-list-item li.focus .list-text a strong,
.cont-list-item li.focus .list-text a span {text-decoration:underline;}
.cont-list-item li .list-text a .search-word  {color:#ec1f2f;}
.cont-list-item li.clip .list-text a:before,
.cont-list-item li.new .list-text a:after {content:'';position:absolute;top:50%;right:-22px;width:16px;height:16px;margin-top:-8px;background:url(../images/content/icon/ico_clip.png) no-repeat center center;cursor:default;}
.cont-list-item li.clip .list-text a:before {right:-22px;}
.cont-list-item li.new .list-text a:after {right:-22px;background-image:url(../images/content/icon/ico_new.png);}
.cont-list-item li.clip.new .list-text a:after {right:-44px;}
.cont-list-item li .list-text a strong {display:inline-block;color:#000;}
.cont-list-item li .list-text a strong.keyword {color:#ec1f2f;}
.cont-list-item li .list-date {display:table-cell;width:16%;font-size:15px;color:#7e7e7e;line-height:22px;text-align:center;vertical-align:middle;}
.cont-list-item.no-result {margin-bottom:30px;border-bottom:1px solid #ddd;}
.cont-list-item.no-result li {border:0;}
.cont-list-item.no-result li p {padding:226px 0 144px;margin-top:20px;background:url(../images/content/icon/ico_no_result.png) no-repeat center 150px;font-size:18px;color:#7e7e7e;line-height:27px;letter-spacing:-1.5px;text-align:center;}

.tab-type2 + .cont-list-box {margin-top:40px;}
.cont-list-item.accordion li {display:block;border:0;}
.cont-list-item.accordion li .accordion-box {display:table;width:100%;min-height:70px;border-bottom:1px solid #ddd;cursor:pointer;}
.cont-list-item.accordion li .accordion-box span {display:table-cell;width:5%;text-align:right;vertical-align:middle;}
.cont-list-item.accordion li .accordion-box .list-text {position:relative;width:95%;padding:0 79px 0 30px;}
.cont-list-item.accordion li .accordion-box .list-text:after {content:'';position:absolute;top:35px;right:31px;width:18px;height:10px;margin-top:-5px;background:url(../images/content/icon/ico_accordion_default.png) no-repeat center center;}
.cont-list-item.accordion li.on .accordion-box .list-text:after {background:url(../images/content/icon/ico_accordion_open.png) no-repeat center center;}
.cont-list-item.accordion li .cont-accordion {display:none;background:#f7f7f7;border-bottom:1px solid #ddd;}
.cont-list-item.accordion li .cont-accordion .answer-text {position:relative;padding:25px 64px;font-size:15px;color:#444;line-height:25px;}
.cont-list-item.accordion li .cont-accordion .answer-text:after {display:block;content:'';clear:both;position:absolute;top:32px;left:29px;width:14px;height:15px;background:url(../images/content/img_answer.png) no-repeat 0 0;}

.cont-list-box2 {margin-top:30px;}
.cont-list-detail {border-top:2px solid #000;}
