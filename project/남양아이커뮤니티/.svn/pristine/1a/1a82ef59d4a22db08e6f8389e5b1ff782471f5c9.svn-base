.smart-schedule-list .smart-list-item.box-red .date .keyword {color:#444;}
.smart-schedule-list .smart-list-item .date {display:table-cell;width:28%;padding-left:19px;font-size:17px;color:#000;line-height:28px;vertical-align:middle;}
.smart-schedule-list .smart-list-item .date .keyword {display:block;font-size:10px;font-weight:700;color:#7e7e7e;line-height:28px;letter-spacing:-1.5px;}
.smart-schedule-list .smart-list-item .date strong {display:block;font-weight:700;}
.smart-schedule-list .smart-list-item .date .keyword + strong {margin-top:-13px;}
.smart-schedule-list .smart-list-item.box-red .date strong {color:#ec1f2f;}
.smart-schedule-list .smart-list-item .schedule-info {display:table-cell;width:72%;padding:0 21px 0 14px;font-size:13px;color:#444;line-height:20px;letter-spacing:-1.5px;vertical-align:middle;}