}
.sub-section3 .stit {
	padding-bottom:55px;
	font-size:32px;
	font-weight: 800;
	color:#333333;
}
.sub-section3 .qna-wrap ul {width:100%;}
.sub-section3 .qna-wrap ul li {overflow:hidden;}
.sub-section3 .qna-wrap ul li + li {margin-top: 23px;}
.sub-section3 .qna-wrap ul li .question-text {
	float:left;
	display:inline-block;
	padding:0 33px;
	background:#f4df31;
	border-radius:30px;
