					$this.addClass('submain-item-idx-' + index);
					if (slideLength == 1) {
						options = {
							loop: false,
							simulateTouch: false,
							slidesPerGroup: 2,
							slidesPerView: 2,
							spaceBetween: 10,
						},
							$('.submain-item-idx-' + index + ' .swiper-button-prev, .submain-item-idx-' + index + ' .swiper-button-next').hide();
