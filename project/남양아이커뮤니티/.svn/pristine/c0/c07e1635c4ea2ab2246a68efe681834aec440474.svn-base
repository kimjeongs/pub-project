.storybook-design .design-area {display:table-cell;width:638px;padding:22px 22px 18px;background-color:#f2f2f2;border-radius:0 7px 7px 0;}
.book-cover:after {display:block;clear:both;content:'';}
.book-cover li {float:left;margin-left:10px;}
.book-cover li:first-child {margin-left:0;}
.image-zoom {display:inline-block;position:relative;}
.image-zoom .img {border:1px solid #ddd;}
.image-zoom .cover-img .f-img {position:absolute;top:0;right:0;width:96px;height:146px;}
.image-zoom .cover-img .b-img img,
.image-zoom .cover-img .f-img img {width:100%;height:100%;}
.image-zoom .cover-img .b-img {position:absolute;top:0;left:0;}
.image-zoom .cover-img.type2 .f-img {top:25px;right:21px;width:59px;height:75px;}
.image-zoom .cover-img.type3 .f-img {top:31px;right:13px;width:75px;height:59px;}
.image-zoom .cover-img.type3 .b-img {top:19px;left:21px;width:59px;height:43px;}
.image-zoom .cover-img.type4 .f-img {top:30px;right:0;width:101px;height:60px;}
.image-zoom .cover-img.type4 .b-img {top:19px;left:13px;width:75px;height:43px;}
.image-zoom .btn-zoom {position:absolute;bottom:10px;right:10px;}
.image-zoom .btn-zoom span {display:inline-block;}
.book-cover li .image-title {font-size:14px;line-height:32px;color:#333;text-align:center;}
.color-pick-wrap,
.book-size-info-wrap {margin-top:20px;border:1px solid #ddd;border-radius:7px;}
.color-pick-wrap .color-title,
.pexel-info .title {font-size:18px;line-height:32px;color:#000;font-weight:700;}
.color-pick-wrap .color-info {display:block;margin-top:5px;font-size:14px;line-height:22px;color:#444;}
.color-pick-wrap .color-pink,
.size-info {padding:35px;}
.color-chart {margin-top:25px;}
.color-chart .radio-set {overflow:hidden;}
.color-chart li {float:left;margin-left:20px;}
.color-chart li:first-child {margin-left:0;}
.color-chart li span {display:inline-block;width:40px;height:40px;border-radius:50%;}
.color-chart li span.red {background-color:#d93239;}
.color-chart li span.orange {background-color:#f48222;}
.color-chart li span.yellow {background-color:#f6c624;}
.color-chart li span.green {background-color:#a3ce62;}
.color-chart li span.aqua {background-color:#6dc8b7;}
.color-chart li span.blue {background-color:#76bfea;}
.color-chart li span.purple {background-color:#c897c6;}
.color-chart li span.pink {background-color:#e671a5;}
.color-chart li label strong {display:block;text-indent:101%;overflow:hidden;white-space:nowrap;}
.color-pick-wrap .sample-view {display:table;width:100%;padding:25px 35px;background-color:#f2f2f2;border-top:1px solid #ddd;border-radius:0 0 7px 7px;}
.color-pick-wrap .sample-view .text-left {display:table-cell;vertical-align:top;}
.color-pick-wrap .preview-wrap {display:table-cell;text-align:right;}
.color-pick-wrap .preview {display:none;overflow:hidden;}
.color-pick-wrap .preview.on {display:block;}
.color-pick-wrap .preview .book-cover {float:right;}

.size-info {display:table;width:100%;}
.size-info dl {display:table-cell;}
.size-info dt,
.size-info dd {display:block;float:left;}
.size-info dt {width:80px;font-size:15px;line-height:25px;color:#000;clear:both;}
.size-info dd {font-size:15px;line-height:25px;color:#444;}
.paper-size {display:table-cell;text-align:right;vertical-align:bottom;}
.paper-size .img {display:inline-block;position:relative;margin-left:27px;vertical-align:bottom;}
.paper-size .img .size-text {position:absolute;top:50%;left:50%;transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%);-moz-transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);white-space:nowrap;}
.paper-size .img:first-child .size-text {margin-left:4px;}
.paper-size .img .size-text span {display:block;margin-top:20px;font-size:15px;line-height:21px;color:#000;text-align:center;}
.paper-size .img .size-text span:first-child {margin-top:0;}
.pexel-info {padding:35px;background-color:#f2f2f2;border-top:1px solid #ddd;border-radius:0 0 7px 7px;}
.pexel-info ul {overflow:hidden;margin-top:26px;}
.pexel-info li {float:left;width:25%;text-align:center;}
.pexel-info li .img {margin-bottom:15px;}
.pexel-info li .text-area {display:inline-block;}
.pexel-info li .pexel-text {text-align:left;}
.pexel-info li .pexel-text strong {display:inline-block;width:55px;font-size:13px;line-height:20px;color:#000;}
.pexel-info li .pexel-text span {font-size:13px;line-height:20px;color:#444;}

/* FOSTO_03_20 */
.text-guide {font-size:14px;line-height:24px;color:#444;}
.text-guide .area {margin-top:15px;}
.text-guide .area.hasbtn {line-height:35px;}
.text-guide strong {font-weight:400;color:#ec1f2f;}
.text-guide em {font-weight:700;color:#000;}
.text-guide .btn {display:inline-block;height:25px;padding:0 10px;margin:0 5px;border-radius:5px;background-color:#fff;border:1px solid #000;font-size:12px;line-height:25px;color:#000;}
.text-guide .red {background-color:#ec1f2f;border:0;color:#fff;}
.text-guide .black {background-color:#333;color:#fff;border:0;}
.text-guide .round {border-radius:25px;}
.text-guide .publication {margin:0 3px;}
.text-guide .add {padding-left:20px;background:url(../images/content/icon/ico_add_plus.png) #ec1f2f no-repeat 10px center;background-size:7px;}
.text-guide .inp-chk-box {display:inline-block;float:none;margin:0 0 0 5px;vertical-align:middle;}
.text-guide .inp-chk + label {padding-left:25px;pointer-events:none;cursor:pointer;}
.text-guide .circle-switch-box {margin-left:10px;pointer-events:none;cursor:pointer;}
.img-guide {margin-top:15px;}
.img-guide + .img-guide {margin-top:10px;}
.img-guide + .text-guide {margin-top:20px;}

.quiesce-cont {position:absolute;top:50%;left:50%;width:930px;text-align:center;transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);-moz-transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%);}
.quiesce-cont .top-title {font-size:38px;line-height:52px;color:#000;font-weight:700;}
.quiesce-cont .top-title strong {color:#ec1f2f;font-weight:700;}
.quiesce-cont .sub-text {margin-top:15px;font-size:17px;line-height:26px;color:#444;}
.quiesce-cont .mail-info-box {margin-top:35px;padding:45px 0;background-color:#f2f2f2;border-radius:7px;}
.quiesce-cont .mail-info-box .list-text:first-child {margin-top:0;}
.quiesce-cont .mail-info-box .list-text {margin-top:25px;font-size:16px;line-height:20px;color:#444;}
.quiesce-cont .mail-info-box .mail-add {margin-top:10px;font-size:24px;line-height:26px;color:#000;font-weight:700;}
.quiesce-cont .radio-set {margin-top:10px;}
.quiesce-cont .radio-set .ipt-rdo + label {font-size:15px;color:#444;}
.quiesce-cont .bullet-text {margin-top:25px;}
.quiesce-cont .bullet-text li {font-size:14px;line-height:24px;color:#7e7e7e;text-align:left;}