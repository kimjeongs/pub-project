				$(this).find('a').attr('href', 'javascript: myScrolls.scrollToElement(document.querySelector(".i-scroll li:nth-child(' + depth3Idx + ')"), null, -15, true)');
				setTimeout(function () {
					$('.i-scroll li:nth-child(' + depth3Idx + ')').find('a').attr('href', "#");
				});
			});
		});
