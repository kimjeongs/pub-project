.contents-wrap {
	background:url('../images/common/bg_contents.png') repeat-x;
}
.contents-inner {
	width:1200px;
	margin:0 auto;
}

/*회원가입 인증 선택*/
.certif-wrap {
    overflow:hidden;
}
.certif-box {
    float:left;
    width:580px;
    padding:60px;
    border:1px solid #dcdcdc;
    border-top:1px solid #8a84d7;
}
.certif-box:last-child {
    float:right;
}
.certif-box .tit {
    font-size:28px;
    font-weight: bold;
}
.certif-box .form-box {
    border-top:1px solid #222;
    border-bottom:1px solid #dcdcdc;
    padding:30px 0;
}
.certif-box .form-box label {
    display:inline-block;
    font-size:16px;
    width:110px;
    
}