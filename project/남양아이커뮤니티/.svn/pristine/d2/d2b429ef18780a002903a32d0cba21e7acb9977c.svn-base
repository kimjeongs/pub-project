/* content */
.item-list {border:1px solid #dfdfdf;border-radius:8px;}
.item-img {position:relative;border-radius:7px;}
.none-bd .item-img .img {height:auto;}
.none-bd .item-img .img img {position:static;-webkit-transform:none;-moz-transform:none;-ms-transform:none;transform:none;}
.item-img .add-check {position:absolute;top:8px;right:8px;overflow:hidden;width:17px;height:21px;}
.item-img .add-check:focus {outline:none;}
.item-img .add-check.on img {margin-top:-21px;}
.none-bd .item-img .add-check {width:18px;height:22px;}
.none-bd .item-img .add-check.on img {margin-top:-23px;}
.item-img .ico-balloon {position:absolute;bottom:4px;left:-18px;}
.item-text {padding:0 30px 30px;}
.none-bd .item-text {padding:20px 10px 0;}
.none-img .item-text {padding-top:26px;}

.round-link-box {position:relative;border-radius:7px;}
/* .round-link-box:after {content:'';position:absolute;top:20px;right:-9px;width:177px;height:92px;background:url(../images/submain/bg_swiper_top.png) no-repeat left top;background-size:100%;} */
.submain-swiper-sub .round-link-box > a {display:block;}
.round-link-box .img {display:block;}
.round-link-box .img img {width:100%;}
.round-link-box .round-link-textbox {position:absolute;top:0;left:0;padding:20px 176px 20px 20px;}
.round-link-box .submain-swiper-title {font-size:15px;font-weight:700;color:#000;line-height:28px;}
.round-link-box .submain-swiper-desc {font-size:11px;color:#444;line-height:16px;}
.round-link-box .btn-common-link {margin-top:13px;}

.post-list-box {max-width:330px;margin:0 auto;}
.post-list {margin:-10px -5px 0;}
.post-list:after{display:block;clear:both;content:'';}
.post-list li {position:relative;float:left;width:50%;margin-top:10px;padding:0 5px;}
.post-list li:nth-child(odd) {clear:both;}
.post-list li .item-list {position:relative;padding-bottom:35px;height:240px !important;background:#fff;}
.post-list li .item-list .item-img{overflow:hidden;border-radius:7px 7px 0 0;}
.post-list li .item-list.none-img .item-img{overflow:visible;}
.post-list .img {width:100%;height:114px;}
.post-list .img img{position:absolute;top:50%;left:50%;-webkit-transform:translate(-50%, -50%);-moz-transform:translate(-50%, -50%);-ms-transform:translate(-50%, -50%);transform:translate(-50%, -50%);width:100%;max-width:none;}
.post-list .top-comment{margin-bottom:3px;font-size:11px;color:#444;text-overflow: ellipsis;width:100%;overflow: hidden;white-space: nowrap;}
.post-list .top-comment .ico {display:inline-block;width:9px;height:10px;margin-right:4px;vertical-align:top;}
.post-list .top-comment .ico img {padding:5px 0;}
.post-list .item-title{overflow:hidden;height:40px;font-size:14px;line-height:20px;font-weight:700;}
.post-list .item-title a {display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;text-overflow: ellipsis;letter-spacing:-1.5px;}
.post-list .item-text{padding:14px;}
.post-list .item-text .btm-like-box{position:absolute;bottom:14px;left:14px; margin-top: 0;}
.post-list .item-detail{overflow: hidden;margin-top:8px;}
.post-list .item-detail a {display: -webkit-box;-webkit-line-clamp: 5;font-size:11px;line-height:18px;color:#7e7e7e;-webkit-box-orient: vertical;text-overflow: ellipsis;}
.post-list .item-text .btm-calendar-box {position:absolute; left: 15px; bottom: 20px; margin-top:13px;padding-left:20px;font-size:10px;color:#444;line-height:15px;letter-spacing:0;}
.post-list .item-text .btm-calendar-box:before {content:'';position:absolute;top:5px;left:0;width:15px;height:15px;margin-top:-5px;background:url(../images/content/icon/ico_calendar2.png) no-repeat; background-size: 15px;}


.board-info {margin-top:33px;}
.board-info .s-title{font-size:22px;line-height:24px;font-weight:700;color:#000;}
.board-info .info-txt{font-size:14px;line-height:22px;color:#444;margin-top:10px;}
.board-info .info-txt.ess {position:relative;padding-left:10px;}
.board-info .info-txt.ess:after {display:block;position:absolute;top:2px;left:0;content:'*';color:#ed3b46;height:10px;}
.board-info2 {padding-top:83px;}
.board-info2 .s-title{font-size:16px;font-weight:700;color:#000;letter-spacing:-0.5px;}
.board-info2 .info-txt{font-size:11px;line-height:16px;color:#444;margin-top:5px;letter-spacing:-0.5px;}
.board-info2.bg-cal{background:url(../images/content/img_calculator.png) no-repeat center top; background-size: 85px 68px;text-align: center;}
.board-info2.bg-baby{background:url(../images/content/img_baby.png) no-repeat center top; background-size: 85px 68px; text-align: center;}

/* top-box */
.top-box {display: inline-block; width: 100%;padding: 15px 15px 0; }
.top-box .inp-select-box{float: left; width: 85px;}
.top-box .sns-share-box{float: right; margin-top: 12px;}

/* bottom-event */
.bottom-event{margin-top: 25px;background: #f6f6f6; border-bottom: 1px solid #ddd;overflow: hidden;}
.bottom-event>a{float: left; width: 50%; height: 70px; padding: 16px 15px;}
.bottom-event>a+a{position: relative; }
.bottom-event>a+a:before{position: absolute; top:17px; left: 0; display: block; width: 1px; height: 40px; background:#ddd;content: ''; }
.bottom-event>a.happy-box{background: url('../images/content/img_btm_evnt01.png') no-repeat 92% center; background-size: 33px 36px;}
.bottom-event>a.baby-class{background: url('../images/content/img_btm_evnt02.png') no-repeat 92% center; background-size: 40px 30px;}
.bottom-event>a span{font-size: 11px; color: #000; letter-spacing: -0.5px;}
.bottom-event>a span strong{display: block; font-size: 15px; font-weight: 700; line-height: 18px;letter-spacing: -0.5px;}

/* SNS Share Button */
.content .c-title + .sns-share-box {position: absolute; top:0; right:15px;}
.sns-share-box {position:relative;}
.sns-share-cont {position:absolute;top:37px;right:5px;z-index:10;visibility:hidden;opacity:0;width:124px;height:129px;margin-left:-72px;-webkit-transition:visibility 0s .3s, opacity .3s ease;-moz-transition:visibility 0s .3s, opacity .3s ease;-ms-transition:visibility 0s .3s, opacity .3s ease;transition:visibility 0s .3s, opacity .3s ease;}
.btn-gnb-share.on + .sns-share-cont,
.btn-share.on + .sns-share-cont {-webkit-transition:opacity .3s ease;-moz-transition:opacity .3s ease;-ms-transition:opacity .3s ease;transition:opacity .3s ease;visibility:visible;opacity:1;}
.sns-share-list {position:absolute;top:0;bottom:0;left:0;right:0;z-index:10;margin-top:5px;padding:14px 15px;border:1px solid #b0b0b0;border-radius:7px;background:rgba(255,255,255,.9);}
.sns-share-list:before {content:'';position:absolute;top:-5px;right:6px;z-index:5;width:8px;height:5px;background:url(../images/content/icon/ico_triangle.png) no-repeat; background-size:8px 5px;}
.ios .sns-share-list:before {right:8px;}
.sns-share-list li a {position:relative;padding-left:20px;font-size:12px;color:#000;line-height:25px;}
.sns-share-list li a span {position:absolute;top:15px;left:0;overflow:hidden;display:inline-block;width:12px;height:12px;margin:-9px 12px 0 0;vertical-align:top; text-align: center;}
.sns-share-list li a span img{height: 100%;}
.sns-share-cont .btn-close {position:absolute;top:-31px;right:-6px;height:40px;padding:14px 11px;background:#fff;}
.sns-share-cont .btn-close span {display:inline-block;width: 12px; height: 12px; vertical-align:top;}
.content .sns-share-cont {right:40px;z-index:15;text-align:left;}
.content .sns-share-cont .btn-close {right:-1px;padding:14px 6px;}
.ios .content .sns-share-cont {right:38px;}
.ios .content .sns-share-cont .btn-close {right:-1px;padding:14px 6px;}
.content .gnb-right-box {float:none;position:relative;margin:0;text-align:right;}
.content .gnb-right-box.bgtype,
.bgtype .sns-share-cont .btn-close {background-color:#f6f6f6;}
.content .btn-gnb-share .text {display:inline-block;width:auto;height:auto;margin-left:5px;font-size:12px;line-height:19px;}
.content .gnb-right-box .btn {padding-right:15px;}

/* 좋아요 */
.btm-like-box {margin-top:13px;font-size:0;}
.item-text .item-desc + .btm-like-box {margin-top:23px;}
.btm-like-box button {position:relative;display:inline-block;margin-left:9px;padding-left:23px;font-size:10px;line-height:20px;pointer-events:none;}
.btm-like-box button span {padding-right:5px;}
.btm-like-box button:before {content:'';position:absolute;top:50%;left:0;width:18px;height:16px;margin-top:-8px;background:url(../images/common/icon/ico_like.png) no-repeat center top; background-size:100%;}
.btm-like-box .btn-like.on:before {background-position:center top;}
.btm-like-box .btn-reply:before {width:17px;height:19px;background-image:url(../images/common/icon/ico_reply.png);}
.btm-like-box button:first-child {margin-left:0;}

/* horizontal btn list */
.hor-tab-box{margin-top:20px;}
.hor-tab:after{display:block;clear:both;content:'';}
.hor-tab li{float:left;width:50%;height:40px;}
.hor-tab li a,
.hor-tab li button{display:block;width:100%;border:1px solid #ddd;font-size:13px;color:#000;line-height:38px;text-align:center;overflow:hidden;}
.hor-tab li .on,
.hor-tab .on button{position:relative;border-color:#000;background-color:#000;color:#fff;}
.hor-tab li .on:before {content:'';overflow:hidden;position:absolute;top:-1px;bottom:-1px;left:-1px;right:-1px;border:3px solid #000;}
.hor-tab li:first-child .on:before {border-radius:7px 0 0 7px;}
.hor-tab li:first-child a,
.hor-tab li:first-child button{border-radius: 7px 0 0 7px;}
.hor-tab li:last-child .on:before {border-radius:0 7px 7px 0;}
.hor-tab li:last-child a,
.hor-tab li:last-child button{border-radius: 0 7px 7px 0;}
.hor-tab.s-type li {width:100%;}
.hor-tab.s-type li a {border-radius:7px;}
.hor-tab.type4 {position:relative;border-radius:7px;background-color:#f6f6f6;}
.hor-tab.type4:before {display:block;position:absolute;top:-1px;left:-1px;border:1px solid #ddd;width:100%;height:100%;clear:both;content:'';border-radius:7px;z-index:-1;}
.hor-tab.type4 li a,
.hor-tab.type4 li button {line-height:40px;}
.hor-tab.type4 li {border-top:1px solid #ddd;border-left:1px solid #ddd;}
.hor-tab.type4 li a,
.hor-tab.type4 li button {border:0;background-color:#fff;}
.hor-tab.type4 li .on {background-color:#000;}
.hor-tab.type4 li:first-child,
.hor-tab.type4 li:nth-child(2) {border-top:0;}
.hor-tab.type4 li:nth-child(odd) {border-left:0;}

.hor-tab.type4 li .on,
.hor-tab.type4 .on button{position:relative;border-color:#000;background-color:#000;color:#fff;}
.hor-tab.type4 li .on:before {content:'';overflow:hidden;position:absolute;top:-1px;bottom:-1px;left:-1px;right:-1px;border:3px solid #000;}

.hor-tab.type4 li:first-child, 
.hor-tab.type4 li:first-child a,
.hor-tab.type4 li:first-child a:before{border-radius:7px 0 0 0;}
.hor-tab.type4 li:nth-child(2),
.hor-tab.type4 li:nth-child(2) a,
.hor-tab.type4 li:nth-child(2) a:before{border-radius:0 7px 0 0;}
.hor-tab.type4 li:nth-child(3),
.hor-tab.type4 li:nth-child(3) a,
.hor-tab.type4 li:nth-child(3) a:before{border-radius:0 0 0 7px;}
.hor-tab.type4 li:last-child,
.hor-tab.type4 li:last-child a,
.hor-tab.type4 li:last-child a:before {border-radius:0 0 7px 0;}

