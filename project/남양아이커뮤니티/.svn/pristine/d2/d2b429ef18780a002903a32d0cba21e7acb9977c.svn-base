.reject-cont .reject-check li .check-txt {display: table-cell}
.reject-cont .reject-check li .check-txt span {font-size: 13px; font-weight: 700; color: #000;font-weight: 700;}
.reject-cont .reject-check li .check-txt li+li {margin-top: 7px;}
.reject-cont .dot-list {margin-top: 12px;}
.reject-cont .dot-list li{color:#7e7e7e;}
.reject-cont .dot-list li+li{margin-top: 2px;}

.class-box .items .img-cont {position:relative;}
.class-box .items .img-cont .gift-txt{position: absolute; left:0; bottom:0; width: 100%; height:25px; background: rgba(255,255,255,.9); text-align: center;}
.class-box .items .img-cont .gift-txt p {font-size:10px;line-height: 25px;color:#333;}
.class-box .items .img-cont .gift-txt p span {color:#ec1f2f;}
.class-box .items .class-list-txt {margin-top:5px;}
.class-box .items .class-list-txt li {font-size: 13px;line-height: 19px;color: #444;}
.class-box .items.type3 .class-detail {color:#000;}

.class-box.flex-box .items > li {float: none;width:100%;padding: 0;overflow: hidden;}
.class-box.flex-box .items > li >div{float: left;width:50%;padding: 0 5px;}