/* smart calendar */
.myroom-title-box.calendar .c-title-point .tooltip-box-position {right:-105px;}
.myroom-title-box.calendar .c-title-point .ico {margin-right:0;font-size:13px;color:#000;line-height:17px;}
.myroom-title-box.calendar .c-title-point .ico img {margin-right:4px;}
.myroom-title-box.calendar .tooltip-box-cont {text-align:left;}
.myroom-title-box.calendar .tooltip-box-cont strong {display:inline-block;margin-top:5px;}

.smart-calendar-top {margin-top:25px;}
.smart-calendar-top {overflow:hidden;display:table;width:100%;padding:30px 39px;border:1px solid #ddd;border-radius:7px;}
.smart-calendar-top .calendar-top-left {display:table-cell;font-size:20px;color:#444;line-height:34px;vertical-align:middle;}
.smart-calendar-top .calendar-top-left strong {font-weight:700;color:#000;}
.smart-calendar-top .calendar-top-right {display:table-cell;text-align:right;vertical-align:middle;}
.smart-calendar-top .calendar-top-right .ico {display:inline-block;vertical-align:middle;}
.smart-calendar-top .calendar-top-right .schedule-num-info {display:inline-block;margin-left:5px;font-size:15px;vertical-align:middle;}
.smart-calendar-top .calendar-top-right .schedule-num-info .num {color:#ec1f2f;text-decoration:underline;}
.smart-calendar-top .calendar-top-right .btn-schedule-add {display:inline-block;margin:0 0 0 14px;vertical-align:middle;}
.smart-calendar-top .calendar-top-right .btn-schedule-add span {position:relative;padding-left:15px;}
.smart-calendar-top .calendar-top-right .btn-schedule-add span:before {content:'';position:absolute;top:50%;left:0;width:10px;height:10px;margin-top:-5px;background-image:url(../images/content/icon/ico_schedule_add.png);}

.smart-calendar-area {margin-top:45px;}
.smart-calendar-area .calendar-date-area {text-align:center;}
.smart-calendar-area .calendar-date-area button,
.smart-calendar-area .calendar-date-area div {display:inline-block;vertical-align:middle;}
.smart-calendar-area .calendar-date-area button {width:30px;height:30px;border:1px solid #ddd;border-radius:7px;}
.smart-calendar-area .calendar-date-area button span {display:inline-block;}
.smart-calendar-area .calendar-date-area .calendar-date-info {padding:0 17px;font-size:28px;color:#000;line-height:40px;}
.smart-calendar-area .benefit-box {padding:0;}
.smart-calendar-area .benefit-box .app-box {display:table;width:100%;padding:35px;background:#f6f6f6;}
.smart-calendar-area .benefit-box .app-box .app-info {display:table-cell;}
.smart-calendar-area .benefit-box .app-box .app-info strong {font-size:17px;color:#000;}
.smart-calendar-area .benefit-box .app-box .app-info span {font-size:13px;color:#444;line-height:20px;}
.smart-calendar-area .benefit-box .app-box .app-install-box {float:none;display:table-cell;padding-left:60px;vertical-align:top;}
.smart-calendar-area .benefit-box .app-box .app-store-sub {display:inline-block;vertical-align:top;}
.smart-calendar-area .benefit-box .app-box .app-store-sub strong {font-weight:700;color:#000;}
.smart-calendar-area .benefit-box .app-box .app-store-sub p {color:#666;}
.smart-calendar-area .benefit-box .app-box .app-store {margin-left:25px;}
.smart-calendar-area .benefit-box .app-box .qr-code {margin-left:40px;}
.smart-calendar-area .benefit-box .app-box .qr-code .img {margin-left:28px;}


.btn-alarm.on + .toggle-alarm-cont {height:235px;}
.toggle-alarm-cont {top:40px;margin-left:-33px;white-space:nowrap;}
.toggle-alarm-cont .alarm-cont-box {padding:18px 24px;text-align:left;}
.toggle-alarm-cont .alarm-cont-box .toggle-alarm-title {font-size:15px;color:#000;line-height:30px;}
.toggle-alarm-cont .alarm-cont-box .alarm-list-title {margin-top:2px;font-size:14px;color:#000;line-height:28px;}
.toggle-alarm-cont .btn-close {top:19px;right:12px;}
.alarm-list-item .alarm-item-title {display:inline-block;width:87px;font-size:13px;color:#444;line-height:20px;vertical-align:middle;}
.circle-switch-box span {position:relative;display:inline-block;;width:30px;height:14px;border-radius:7px;background:#aaa;font-size:0;line-height:0;vertical-align:middle;cursor:pointer;}
.circle-switch-box.on span {background:#ec1f2f;}
.circle-switch-box span:before {content:'';position:absolute;top:50%;left:0;-webkit-transition:all .2s ease-in-out;-moz-transition:all .2s ease-in-out;-ms-transition:all .2s ease-in-out;transition:all .2s ease-in-out;width:14px;height:14px;margin-top:-9px;border:2px solid #aaa;border-radius:50%;background:#fff;}
.circle-switch-box.on span:before {left:12px;-webkit-transition:all .2s ease-in-out;-moz-transition:all .2s ease-in-out;-ms-transition:all .2s ease-in-out;transition:all .2s ease-in-out;border-color:#ec1f2f;}
.alarm-list-cont > ul li .alarm-list-item .alarm-item-title {position:relative;padding-left:12px;color:#7e7e7e;}
.alarm-list-cont > ul li .alarm-list-item .alarm-item-title:before {content:'';position:absolute;top:50%;left:0;width:6px;height:6px;margin-top:-3px;background-image:url(../images/content/icon/ico_sub_switch.png);}

.calendar-cont-top {display:table;width:100%;margin-top:25px;}
.all-schedule-box {position:relative;display:table-cell;vertical-align:middle;}
.all-schedule-box .btn-all-schedule {width:160px;border:1px solid #ddd;border-radius:7px;background:url(../images/content/icon/ico_down_arrow.png) no-repeat right center;text-align:left;cursor:default;}
.all-schedule-box .btn-all-schedule.on {border-radius:7px 7px 0 0;background-image:url(../images/content/icon/ico_up_arrow2.png);}
.all-schedule-box .btn-all-schedule span {overflow:hidden;display:inline-block;width:100%;padding:0 26px 0 14px;font-size:14px;color:#000;line-height:36px;text-overflow:ellipsis;word-wrap:normal;white-space:nowrap;}
.all-schedule-box .btn-all-schedule span.placeholder {color:#aaa;}
.all-schedule-box .option-schedule-item {position:absolute;top:38px;left:0;z-index:-1;opacity:0;transition:all .2s ease-in-out;width:160px;border:1px solid #ddd;border-radius:0 0 7px 7px;}
.all-schedule-box .option-schedule-item ul {margin-top:-1px;}
.all-schedule-box .option-schedule-item ul li {overflow:hidden;padding:0 14px;border-top:1px solid #fff;background:#ddd;font-size:14px;color:#000;line-height:38px;text-overflow:ellipsis;word-wrap:normal;white-space:nowrap;cursor:default;}
.all-schedule-box .option-schedule-item ul li:first-child {border-top:0;line-height:39px;}
.all-schedule-box .option-schedule-item ul li:nth-child(2) {background-color:#ffc6cd;}
.all-schedule-box .option-schedule-item ul li:nth-child(3) {background-color:#fcd7ba;}
.all-schedule-box .option-schedule-item ul li:nth-child(4) {background-color:#fcecae;}
.all-schedule-box .option-schedule-item ul li:nth-child(5) {background-color:#e2f6ae;}
.all-schedule-box .option-schedule-item ul li:nth-child(6) {background-color:#cfdeff;}
.all-schedule-box .btn-all-schedule.on + .option-schedule-item {opacity:1;z-index:5;transition:all .2s ease-in-out;}
.calendar-cont-top .all-schedule-options {display:table-cell;padding-bottom:4px;text-align:right;vertical-align:bottom;}
.calendar-cont-top .all-schedule-options .all-schedule-list {position:relative;display:inline-block;}
.calendar-cont-top .all-schedule-options .all-schedule-list:after {content:'';display:block;clear:both;}
.calendar-cont-top .all-schedule-options .all-schedule-list li {float:left;}
.calendar-cont-top .all-schedule-options .all-schedule-list li + li {position:relative;margin-left:15px;padding-left:15px;}
.calendar-cont-top .all-schedule-options .all-schedule-list li + li:before {content:'';position:absolute;top:3px;bottom:3px;left:0;width:1px;background:#ddd;}
.all-schedule-cont .all-schedule-title {display:inline-block;font-size:14px;color:#000;line-height:20px;vertical-align:middle;}
.all-schedule-cont .all-schedule-title span {color:#005388;}
.all-schedule-cont .all-schedule-title .btn-tooltip {margin-left:5px;}
.all-schedule-cont .all-schedule-title .tooltip-box-position {top:33px;left:-12px;}
.all-schedule-cont .all-schedule-title .tooltip-box-position .tooltip-box-cont{text-align:left;}
.all-schedule-cont .circle-switch-box {margin-left:9px;}
.circle-switch-box {position:relative;display:inline-block;width:30px;height:20px;vertical-align:middle;}

.calendar-cont-main {margin-top:15px;}
.calendar-cont-main .tbl-smart-calendar {width:100%;border-top:1px solid #000;}
.calendar-cont-main .tbl-smart-calendar thead th {padding:9px 10px;border-bottom:1px solid #cbcbcb;font-size:16px;color:#000;line-height:31px;}
.calendar-cont-main .tbl-smart-calendar tbody td {height:151px;margin:0;padding:0;border-bottom:1px solid #ddd;vertical-align:top;}
.calendar-cont-main .tbl-smart-calendar tbody td > div {position:relative;height:100%;padding:10px;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .btn-schedule-add {padding:0 4px;font-family:"돋움", dotum, Helvetica, sans-serif;font-size:11px;color:#444;line-height:20px;vertical-align:middle;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .date-text {overflow:hidden;display:inline-block;width:78px;font-family:"돋움", dotum, Helvetica, sans-serif;font-size:11px;color:#ec1f2f;line-height:20px;text-overflow:ellipsis;word-wrap:normal;white-space:nowrap;vertical-align:middle;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-schedule,
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-my-schedule {display:none;margin-top:5px;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-schedule + .smart-my-schedule {margin-top:0;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-schedule li > button,
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-my-schedule li > button {vertical-align:top;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-schedule li > button span,
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-my-schedule li > button span {overflow:hidden;display:inline-block;max-width:100px;font-family:"돋움", dotum, Helvetica, sans-serif;font-size:11px;color:#000;line-height:20px;letter-spacing:0;text-overflow:ellipsis;word-wrap:normal;white-space:nowrap;text-align:left;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-schedule li > button span {color:#005388;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-my-schedule .ico {position:relative;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-my-schedule .ico:before {content:'';position:absolute;top:50%;left:0;width:8px;height:8px;margin-top:-5px;border-radius:50%;background:#ddd;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-my-schedule .ico.color1:before {background-color:#ffc6ce;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-my-schedule .ico.color2:before {background-color:#fdd7ba;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-my-schedule .ico.color3:before {background-color:#fcedae;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .smart-my-schedule button {padding-left:12px;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .more-schedule-box .smart-schedule li,
.calendar-cont-main .tbl-smart-calendar tbody td > div .more-schedule-box .smart-my-schedule li {overflow:hidden;display:inline-block;max-width:100%;font-family:"돋움", dotum, Helvetica, sans-serif;font-size:11px;color:#000;line-height:20px;letter-spacing:0;text-overflow:ellipsis;word-wrap:normal;white-space:nowrap;text-align:left;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .more-schedule-box .smart-schedule li {color:#005388;}
.calendar-cont-main .tbl-smart-calendar tbody td > div .tooltip-box-position {z-index:-1;opacity:0;display:block;}
.calendar-cont-main .tbl-smart-calendar tbody td .prev-month,
.calendar-cont-main .tbl-smart-calendar tbody td .next-month {background:#f6f6f6;}
.calendar-cont-main .tbl-smart-calendar tbody td .prev-month .btn-schedule-add,
.calendar-cont-main .tbl-smart-calendar tbody td .next-month .btn-schedule-add {color:#aaa;}
.calendar-cont-main .tbl-smart-calendar tbody td .holiday .btn-schedule-add {color:#ec1f2f;}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day {position:relative;background:#fcecae;}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day .btn-birthday-tooltip {float:right;width:auto;height:auto;background:none;text-indent:unset;white-space:normal;}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day .btn-birthday-tooltip span {display:inline-block;padding-right:21px;background:url(../images/content/icon/ico_birthday.png) no-repeat right top;font-family:"돋움", dotum, Helvetica, sans-serif;font-size:12px;color:#000;line-height:20px;}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day .calendar-birthday-cont {-webkit-transition:all .2s linear;-moz-transition:all .2s linear;-ms-transition:all .2s linear;transition:all .2s linear;position:absolute;top:-8px;right:-132px;z-index:-1;opacity:0;}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day .btn-birthday-tooltip.on + .calendar-birthday-cont {-webkit-transition:opacity .2s linear;-moz-transition:opacity .2s linear;-ms-transition:opacity .2s linear;transition:opacity .2s linear;z-index:1;opacity:1;}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day .calendar-birthday-cont button {background-image:url(../images/content/img_tooltip_close2.png);}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day .calendar-birthday-cont .nicescroll-rails {z-index:1 !important;opacity:1 !important;}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day .calendar-birthday-cont .nicescroll-cursors {left:6px !important;width:3px !important;}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day .calendar-birthday-cont .btn-close {position:absolute;top:0;right:-17px;width:13px;height:13px;font-size:0;line-height:0;}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day .tooltip-box-cont {min-width:130px;border-color:#000;background-color:#fff;color:#333;}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day .tooltip-box-cont ul {overflow:hidden;max-height:66px;}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day .tooltip-box-cont ul li + li {margin-top:4px;}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day .tooltip-box-cont:after {top:22px;left:1px;width:9px;height:17px;background-image:url(../images/content/img_tooltip_tri2.png);}
.calendar-cont-main .tbl-smart-calendar tbody td .birth-day .tooltip-box-cont strong.tooltip-box-title {color:#333;}
.calendar-cont-main .tbl-smart-calendar tbody td .today {position:relative;border:1px solid #333;border-width:0 1px;}
.calendar-cont-main .tbl-smart-calendar tbody td .today:before {content:'';position:absolute;top:-1px;left:-1px;right:-1px;height:1px;background:#333;}
.calendar-cont-main .tbl-smart-calendar tbody td .today:after {content:'';position:absolute;bottom:-1px;left:-1px;right:-1px;height:1px;background:#333;}
.calendar-cont-main .tbl-smart-calendar tbody td .more-view-schedule .btn-more-schedule {position:absolute;bottom:10px;right:0;width:18px;height:18px;border-radius:50%;background:#ec1f2f;font-size:11px;color:#fff;line-height:20px;}
.calendar-cont-main .tbl-smart-calendar tbody td .more-view-schedule .more-schedule-area {position:absolute;bottom:-38px;left:130px;opacity:0;padding:11px 14px;border:1px solid #ec1f2f;border-radius:7px;background:#fff;}
/* .calendar-cont-main .tbl-smart-calendar tbody td .more-view-schedule .btn-more-schedule.on + .more-schedule-area {z-index:1;} */
.calendar-cont-main .tbl-smart-calendar tbody td .more-view-schedule .more-schedule-area:before {content:'';position:absolute;top:50%;left:-8px;width:8px;height:17px;margin-top:-9px;background-image:url(../images/content/img_more_schedule.png);}
.calendar-cont-main .tbl-smart-calendar tbody td .more-view-schedule .more-schedule-area .more-schedule-box {width:211px;height:88px;}
.calendar-cont-main .tbl-smart-calendar tbody td .more-view-schedule .more-schedule-area .nicescroll-cursors {left:3px !important;width:3px !important;}
.calendar-cont-main .tbl-smart-calendar tbody td .more-view-schedule .more-schedule-area .more-schedule-box ul {display:block !important;margin-top:0;}
.calendar-cont-main .tbl-smart-calendar tbody td .more-view-schedule .more-schedule-area .more-schedule-box ul li {line-height:18px;}
.calendar-cont-main .tbl-smart-calendar tbody td .more-view-schedule .more-schedule-area .more-schedule-box ul .ico {padding-left:0;}
.calendar-cont-main .tbl-smart-calendar tbody td .more-view-schedule .more-schedule-area .more-schedule-box ul .ico:before {display:none;}
.calendar-cont-main .tbl-smart-calendar tbody td .more-view-schedule .btn-close {position:absolute;top:0;right:-18px;}
.calendar-cont-main .tbl-smart-calendar tbody td .more-view-schedule .btn-close span {display:inline-block;}
.calendar-btm-banner {margin-top:60px;}