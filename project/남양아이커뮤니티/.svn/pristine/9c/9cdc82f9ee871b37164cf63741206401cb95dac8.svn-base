.storybook-intro-wrap {padding:0 15px;}
.storybook-intro-wrap .banner-text-info {top:30%;}
.storybook-intro-wrap .banner-title {font-weight:700;}
.storybook-set-box {padding:20px;border:1px solid #ddd;border-radius:0 0 7px 7px;text-align:center;}
.storybook-set-box .count {font-size:18px;line-height:25px;font-weight:700;color:#000;}
.storybook-set-box .count em {font-size:12px;color:#7e7e7e;}
.storybook-set-box .info {margin-top:10px;font-size:13px;line-height:18px;color:#7e7e7e;}
.storybook-set-box .btn-box {margin-top:15px;}
.storybook-set-box + .caution {margin-top:10px;}
.special-list li {border:1px solid #ddd;border-radius:7px;margin-top:10px;}
.special-list li .inner {display:table;width:100%;padding:15px;font-size:0;line-height:0;}
.special-list li .inner .img {display:table-cell;width:62px;padding-right:15px;vertical-align:middle;}
.special-list li .inner .text {display:table-cell;vertical-align:middle;}
.special-list li .inner .btn-common-link {margin-top:15px;}
.special-list li .btm {border-radius:0 0 7px 7px;background-color:#f2f2f2;font-size:11px;line-height:35px;text-align:center;}
.storybook-intro-wrap .title-box {margin-top:45px;}
.storybook-intro-wrap .title-box:first-child {margin-top:15px;}
.storybook-intro-wrap .top-banner-cont {position:relative;margin-top:0;}
.storybook-intro-wrap .top-banner-cont .img img {width:100%;}
.connect-btm-wrap .connect-btm-cont {margin-top:15px;}
.connect-btm-wrap .connect-btm-cont:first-child {margin-top:45px;}

.storybook-swiper-wrap {position:relative;margin-top:50px;background:url(../images/content/bg_press_swiper.png) #fff3e8 no-repeat right 16px;background-size:54%;border-radius:7px;overflow:hidden;}
.submain-swiper-left {padding:30px 25px 25px;}
.submain-swiper-left .area-title {font-size:17px;line-height:24px;color:#000;font-weight:700;}
.submain-swiper-left .btn-common-link {margin-top:10px;}
.storybook-press-swipe {width:120%;margin-left:25px;padding:0 0 55px 0 !important;}
.storybook-press-swipe .story-swiper-area {position:relative;padding:25px;background-color:#fff;min-height:205px;border-radius:7px;word-break:keep-all;}
.storybook-press-swipe .story-swiper-area .press-title {font-size:14px;line-height:19px;color:#000;font-weight:700;}
.storybook-press-swipe .story-swiper-area .press-title a {overflow:hidden;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;text-overflow:ellipsis;word-break:break-all;}
.storybook-press-swipe .story-swiper-area .press-inner {margin-top:8px;font-size:11px;line-height:18px;color:#444;}
.storybook-press-swipe .story-swiper-area .press-inner a {overflow:hidden;display:-webkit-box;-webkit-line-clamp:4;-webkit-box-orient:vertical;text-overflow:ellipsis;word-break:break-all;}
.storybook-press-swipe .story-swiper-area .writer {position:absolute;bottom:25px;left:25px;font-size:11px;line-height:20px;color:#7e7e7e;}
.storybook-swiper-wrap .swiper-pagination {bottom:25px;left:50%;transform:translateX(-50%);}

.story-link-box {margin-top:15px;border:1px solid #ddd;border-radius:7px;overflow:hidden;}
.story-link-box li {float:left;width:33.3%;border-left:1px solid #ddd;text-align:center;}
.story-link-box li a {display:block;padding:20px;}
.story-link-box li:first-child {width:33.34%;border-left:0;}
.story-link-box li .img {display:block;height:48px;}
.story-link-box li .btn-common-link {display:block;margin-top:10px;padding:0 0 25px 0;}
.story-link-box li .btn-common-link:after {right:auto;top:auto;bottom:0;left:50%;transform:translateX(-50%);}
.find-storybook {padding:30px 0;border-bottom:1px solid #ddd;text-align:center;}
.find-storybook .btn-common-link {display:inline-block;margin-top:15px;}
.tab-view-cont {position:relative;padding-top:77.3%;}
.tab-view-cont .tab-item {position:absolute;top:0;left:0;right:0;z-index:1;opacity:0;-webkit-transition:opacity .35s ease;-moz-transition:opacity .35s ease;-ms-transition:opacity .35s ease;transition:opacity .35s ease;}
.tab-view-cont .tab-item.active {z-index:10;opacity:1;-webkit-transition:opacity .35s ease;-moz-transition:opacity .35s ease;-ms-transition:opacity .35s ease;transition:opacity .35s ease;}
.storybook-relay .hor-tab-box {margin-top:15px;}
.post-list .id-text,
.storybook-relay .book-press-box .id-text {margin-top:8px;font-size:11px;line-height:12px;color:#7e7e7e;}
.post-list .write-date,
.storybook-relay .book-press-box .write-date {position: absolute;bottom:14px;left:14px;font-size:11px;line-height:12px;color:#7e7e7e;}

.storybook-relay .book-press-box ul li {width:49%;height:175px;padding-top:40px;margin:42px 0 0 2%;}
.storybook-relay .book-press-box ul li:first-child,
.storybook-relay .book-press-box ul li:nth-child(2) {margin-top:0;}
.storybook-relay .book-press-box ul li:nth-child(odd) {clear:both;margin-left:0;}
.storybook-relay .book-press-box ul li.pages:before {width:96%;background-size:100% 5px;}
.baby-pic {position:absolute;top:-27px;left:50%;width:54px;height:54px;border-radius:50%;overflow:hidden;transform:translateX(-50%);}
.storybook-relay .book-press-box ul li .baby-pic img {position:absolute;top:50%;left:50%;max-width:none;height:100%;transform:translate(-50%,-50%);}
.storybook-relay .book-press-box ul {padding-top:27px;}
.storybook-relay .book-press-box .relay-date {margin-bottom:5px;font-size:11px;line-height:12px;color:#000;}
.storybook-relay .book-press-box .relay-date strong {color:#ec1f2f;}
.storybook-relay .top-comment {margin-bottom:3px;font-size:11px;color:#444;text-overflow:ellipsis;width:100%;overflow:hidden;white-space:nowrap;}
.storybook-relay.s-type .book-press-box ul li {width:50%;height:175px;margin-top:0;margin-left:0;padding-top:40px;}
.storybook-relay.s-type .tab-view-cont {padding-top:255px;}
.storybook-relay.s-type .book-press-box ul li .baby-pic img {position:absolute;top:50%;left:50%;max-width:none;height:100%;transform:translate(-50%,-50%);}
.storybook-relay.s-type .hor-tab {overflow:visible;}
.storybook-relay.s-type .hor-tab:after {content:'';display:block;clear:both;}
.storybook-relay.s-type .hor-tab li {overflow:visible;}

.storybook-list-wrap {padding:15px 15px 0;}
.storybook-list-wrap .item-search {margin-top:10px;}
.storybook-list-wrap .count-sort-text {margin-bottom:15px;}
.storybook-list-wrap .empty-cont {margin:0;padding-bottom:40px;border-top:1px solid #000;border-bottom:1px solid #ddd;}
.change-stamp {padding:25px;margin:25px 0 50px 0;background-color:#f9f4e4;border-radius:7px;text-align:center;}
.change-stamp .inner-stamp-box {margin-top:15px;}
.change-stamp .sub-info-text {margin-top:10px;}
.change-stamp .inner-stamp-box .detail {margin-top:10px;}

.special-list li .sub-info-text {margin-top:5px;line-height:16px;word-break:keep-all;}
.order-circle-wrap {margin-top:12px;}
.order-circle-wrap ul {border:1px solid #ddd;border-radius:7px;padding:16px 15px;}
.order-circle-wrap ul li {display:table;width:100%;table-layout:fixed;padding:5px 0;}
.order-circle-wrap ul li .order,
.order-circle-wrap ul li .circle-item {display:table-cell;vertical-align:middle;}
.order-circle-wrap ul li .order {width:10%;text-align:center;}
.order-circle-wrap ul li .item-num {display:block;width:18px;height:18px;background-color:#333;color:#fff;font-size:11px;line-height:18px;border-radius:50%;text-align:center;}
.order-circle-wrap ul li .order-title {font-size:13px;line-height:20px;font-weight:700;color:#000;}
.title-info {margin-top:50px;}
.title-info .sub-info-text {margin-top:5px;}

.nytime-wrap {margin-top:15px;padding:20px 15px;background-color:#f6f6f6;border:1px solid #ddd;border-radius:7px;text-align:center;}
.clock-box {display:inline-block;}
.clock-box:first-child {margin-right:50px;}
.clock-box .img {display:inline-block;vertical-align:middle;}
.clock-box .time-info {display:block;margin-top:10px;font-size:11px;line-height:22px;color:#444;}
.clock-box .time-info strong {display:inline-block;margin-left:5px;font-weight:700;}
.clock-box.nytime .time-info {color:#ec1f2f;}
.nytime-wrap .nytime-title {margin-top:20px;font-size:14px;line-height:21px;color:#000;font-weight:700;}
.nytime-wrap .nytime-detail {margin-top:10px;font-size:12px;line-height:18px;color:#444;}
.nytime-wrap .nytime-ex {margin-top:20px;font-size:11px;line-height:18px;color:#7e7e7e;}

.present-box {margin-top:25px;padding:35px 30px;border:1px solid #ddd;border-radius:7px;background:url(../images/content/img_present.png) no-repeat right 30px top 20px;background-size:74px 62px;}
.present-box .present-coupon {display:inline-block;text-align:left;vertical-align:middle;}
.present-box .sub-info-text {margin-top:15px;}

.way-to-wrap:after {display:block;clear:both;content:'';}
.way-to-wrap + .caution {margin-top:10px;}
.payment-box {padding:25px 15px 25px 90px;margin-top:10px;border:1px solid #ddd;border-radius:7px;}
.payment-box.stamp {background:url(../images/content/img_way_stamp.png) no-repeat 15px center;background-size:62px 46px;}
.payment-box.mall {background:url(../images/content/img_way_mall.png) no-repeat 15px center;background-size:58px 44px;}
.payment-box .way-title {font-size:15px;line-height:22px;color:#222;font-weight:700;}
.payment-box .way-title strong {color:#ec1f2f;}
.payment-box .way-detail {margin-top:5px;font-size:14px;line-height:20px;color:#444;}

.cover-design-wrap .sub-info-text {margin-top:10px;}
.cover-design-wrap .design-type {font-size:14px;line-height:20px;color:#000;}
.cover-design {margin-top:20px;}
.cover-design .img {margin-top:10px;border:1px solid #ddd;}

.color-pick-wrap .preview {display:none;overflow:hidden;}
.color-pick-wrap .preview.on {display:block;}
.color-pick-wrap .preview-wrap {margin-top:30px;padding:25px 15px;border-radius:7px;background-color:#f6f6f6;}
.color-pick-wrap .color-title {font-size:15px;line-height:18px;color:#000;font-weight:700;}
.color-pick-wrap .image-title {margin-bottom:5px;font-size:13px;line-height:18px;color:#000;}
.color-pick-wrap .color-pick-text {margin-top:40px;font-size:17px;line-height:20px;color:#000;font-weight:700;}
.book-cover li {margin-top:25px;}
.book-cover li:first-child {margin-top:10px;}
.image-zoom .img {border:1px solid #ddd;}
.preview-wrap .color-info {display:block;margin-top:20px;font-size:12px;line-height:19px;color:#444;}

.paper-size {margin-top:15px;padding:25px 0;border:1px solid #ddd;border-radius:7px;text-align:center;}
.paper-size .images {display:inline-block;}
.paper-size .img {display:inline-block;position:relative;margin-left:25px;vertical-align:bottom;}
.paper-size .img:first-child {margin-left:0;}
.size-info {margin-top:15px;overflow:hidden;}
.size-info dt,
.size-info dd {display:block;float:left;}
.size-info dt {width:70px;font-size:12px;line-height:24px;color:#000;font-weight:700;clear:both;}
.size-info dd {font-size:12px;line-height:24px;color:#444;}
.paper-size .img .size-text {position:absolute;top:50%;left:50%;transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%);-moz-transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);white-space:nowrap;}
.paper-size .img:first-child .size-text {margin-left:4px;}
.paper-size .img .size-text span {display:block;margin-top:13px;font-size:10px;line-height:15px;color:#000;text-align:center;}
.paper-size .img .size-text span:first-child {margin-top:0;}
.pexel-info {margin-top:35px;}
.pexel-info .title {font-size:15px;line-height:20px;color:#000;}
.pexel-info ul {overflow:hidden;}
.pexel-info li {float:left;width:47%;text-align:center;margin:15px 0 0 6%;}
.pexel-info li:nth-child(odd) {clear:both;margin-left:0;}
.pexel-info li .img {margin-bottom:10px;}
.pexel-info li .pexel-text {text-align:left;}
.pexel-info li .pexel-text strong {display:inline-block;width:50px;font-size:11px;line-height:20px;color:#000;}
.pexel-info li .pexel-text span {font-size:11px;line-height:20px;color:#444;}

.title-box + .text-guide {margin-top:15px;}
.text-guide {font-size:12px;line-height:20px;color:#444;}
.text-guide .area {margin-top:10px;}
.text-guide .area.hasbtn {line-height:29px;}
.text-guide .area.hasbtn .publication {margin:0 3px 0 0;}
.text-guide .area.hasbtn .publication:first-child {margin:0 3px;}
.text-guide .area.hasbtn .btn.red.round {margin-left:5px;}
.text-guide strong {font-weight:400;color:#ec1f2f;}
.text-guide em {font-weight:700;color:#000;}
.text-guide .btn {display:inline-block;height:25px;padding:0 10px;margin-right:5px;border-radius:5px;background-color:#fff;border:1px solid #000;font-size:11px;line-height:25px;color:#000;}
.text-guide .red {background-color:#ec1f2f;border:0;color:#fff;}
.text-guide .black {background-color:#333;color:#fff;border:0;}
.text-guide .round {border-radius:25px;}
.text-guide .add {padding-left:20px;background:url(../images/common/icon/btn_add.png) #ec1f2f no-repeat 10px center;background-size:7px;}
.text-guide .inp-chk-box {display:inline-block;margin:0 5px;vertical-align:top;}
.img-guide {margin-top:23px;}
.img-guide + .img-guide {margin-top:10px;}
.img-guide + .text-guide {margin-top:23px;}

.board-wrap .board-info:first-child {margin-top:25px;padding:0 15px;}
.board-wrap .form-list-txt {margin:15px 15px 0;}
.double-select-btn-area {margin-top:10px;padding-right:58px;position:relative;}
.double-select-btn-area .btn-box {position:absolute;right:0;top:0;margin:0;}
