@import url('lib/swiper.min.css');

/* main */
.container.main {border-top:0;}
.container.main .content.add-pd {padding-bottom:32px;}
.sub-cont {position:relative;padding-bottom:100px;}
.sub-cont .content {padding:80px 0 0;}

/* swiper */
.swiper-wrap {overflow:hidden;margin-top:20px;}
.swiper-container {overflow:visible;max-width:1120px;margin:0 auto;}
.swiper-slide {position:relative;display:-webkit-box;display:-ms-flexbox;display:-webkit-flex;display:flex;-webkit-box-pack:center;-ms-flex-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-ms-flex-align:center;-webkit-align-items:center;align-items:center;max-width:1120px;height:100%;border-radius:10px;text-align:center;font-size:18px;}
/* .main-swiper .swiper-slide {padding-left:270px;} */
.main-swiper {min-height:520px;}
.main-swiper .swiper-slide .img {overflow:hidden;padding-left:270px;border-radius:10px;}
.main-swiper .swiper-slide .img img {border-radius:10px;}
.main-swiper .swiper-slide:before {content:'';position:absolute;top:0;bottom:0;left:270px;right:0;border-radius:10px;background-color:rgba(0,0,0,.1);transition:all ease-in-out .5s;}
.main-swiper .swiper-slide.swiper-slide-active:before {background-color:rgba(0,0,0,0);transition:all ease-in-out .5s;}
.swiper-button-prev,
.swiper-button-next {width:80px;height:80px;margin-top:-40px;padding:15px 24px;border-radius:50%;box-shadow:0 3px 10px 0 rgba(0,0,0,.1);background-image:url(../images/common/icon/ico_swiper_pagination.png);background-color:rgba(255,255,255,.9);background-size:10px 17px;}
.swiper-button-prev {left:-142px;background-image:url(../images/common/icon/ico_swiper_prev.png);}
.swiper-button-next {right:-142px;background-image:url(../images/common/icon/ico_swiper_next.png);}

/*login-box */
.swiper-position {position:relative;width:1600px;margin:0 auto;}
.login-box-wrap {position:absolute;top:0;left:50%;width:330px;margin-left:-560px;padding:94px 20px 0;text-align:left;z-index:10;}
.login-box-wrap .l-title {display:block;font-size:26px;font-weight:700;color:#000;line-height:36px;letter-spacing:-1.5px;}
.login-box-wrap .login-chk-box .btn-login {float:right;width:170px;}
.right-text-box {position:absolute;top:0;right:0;width:850px;padding:124px 0 0 433px;text-align:left;}
.right-text-box .eyebrow {position:relative;display:inline-block;margin-bottom:14px;padding:5px 15px;border-radius:18px;background:#ef4639;font-size:17px;color:#fff;line-height:26px;}
.right-text-box .eyebrow:after {content:'';position:absolute;bottom:0;right:0;width:15px;height:18px;border-radius:0 0 3px 0;background:#ef4639;z-index:0;}
.right-text-box .r-title {display:block;font-size:34px;color:#fff;line-height:46px;}
.right-text-box .r-title strong {font-weight:700;}
.right-text-box .r-desc {display:block;margin-top:13px;font-size:15px;color:#fff;line-height:22px;}
.right-text-box .btn-campaign {position:relative;display:inline-block;margin-top:47px;font-size:14px;color:#fff;line-height:22px;pointer-events:auto;}
.right-text-box .btn-campaign:before {content:'';position:absolute;top:50%;right:-28px;width:11px;height:12px;margin-top:-6px;background:url(../images/main/icon/ico_campaign.png) no-repeat center center;z-index:1;}
.right-text-box .btn-campaign:after {content:'';position:absolute;top:50%;right:-36px;width:28px;height:28px;margin-top:-14px;border-radius:14px;background:#fff;}
.login-box {position:relative;margin-top:22px;}
.login-box:after {content:'';display:block;clear:both;}
.login-box div {position:relative;float:left;border-bottom:2px solid #000;}
.login-box div:before {content:'';position:absolute;top:50%;left:6px;width:18px;height:18px;margin-top:-9px;}
.login-box .inp-box {width:140px;height:40px;padding:4px 10px 4px 32px;font-size:14px;color:#000;line-height:normal;}
.login-box .inp-box::-webkit-input-placeholder {color:#000;}
.login-box .inp-box::placeholder {color:#000;}
.login-box .inp-box:-ms-input-placeholder {color:#000;}
.login-box .error-msg {position:absolute;bottom:-30px;left:0;}
.id-box:before {background-image:url(../images/common/icon/ico_id_box.png);}
.pw-box {position:relative;margin-left:10px;}
.pw-box:before {background-image:url(../images/common/icon/ico_pw_box.png);}
.login-chk-box {margin-top:5px;padding-top:15px;text-align:right;}
.login-box-wrap.error-txt .login-chk-box {padding-top:35px;}
.login-chk-box:after {content:'';display:block;clear:both;}
.inp-chk-box {position:relative;float:left;margin-top:15px;}
.inp-chk {display:none;}
.inp-chk + label {position:relative;display:inline-block;padding-left:30px;font-size:13px;line-height:22px;color:#000;letter-spacing:-1.5px;cursor:pointer;}
.inp-chk + label:before {content:'';position:absolute;top:50%;left:-1px;width:20px;height:20px;margin-top:-12px;border:1px solid #ccc;border-radius:5px;background:#fff;}
.inp-chk + label:after {content:'';position:absolute;top:50%;left:3px;width:14px;height:10px;margin-top:-6px;background:url(../images/common/icon/ico_chk_off.png) no-repeat center center;z-index:2;}
.inp-chk:checked + label:before {border-color:#000;}
.inp-chk:checked + label:after {background-image:url(../images/common/icon/ico_chk_on.png);}
.inp-chk:disabled + label:before{background-color:#eee;}
.inp-chk:disabled + label:after{background-image:url(../images/common/icon/ico_chk_disabled.png);}
.inp-chk:disabled + label {cursor:auto;}
.inp-chk:checked:disabled + label:before{border:1px solid #ddd;}
.inp-chk:checked:disabled + label:after{background-image:url(../images/common/icon/ico_chk_on_disabled.png);}
.underline-text-box {margin-top:14px;font-size:0;}
.underline-text-box li {position:relative;display:inline-block;}
.underline-text-box li + li {margin-left:12px;padding-left:12px;}
.underline-text-box li + li:before {content:'';position:absolute;top:8px;bottom:8px;left:0;width:1px;background:#aaa;}
.underline-text-box li a {font-size:14px;color:#000;line-height:26px;}
.login-chk-box .btn-login {padding:12px 10px;border-radius:23px;background:#ec1f2f;font-size:16px;font-weight:700;color:#fff;line-height:22px;letter-spacing:-1.5px;}

.sns-link-box {position:relative;margin-top:29px;padding:29px 0 30px;}
/* .error-txt .sns-link-box {margin-top:9px;} */
.sns-link-box dl:after {content:'';display:block;clear:both;}
.sns-link-box dl dt,
.sns-link-box dl dd {float:left;}
.sns-link-box dl dt {padding:14px 19px 14px 0;font-size:16px;color:#000;line-height:18px;letter-spacing:-1.5px;}
.sns-link-box dl dd a {width:40px;height:40px;margin-left:6px;}
.sns-link-box dl dd a img {width:100%;height:100%;}

