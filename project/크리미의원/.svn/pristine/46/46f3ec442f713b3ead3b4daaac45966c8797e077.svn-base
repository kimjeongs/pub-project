* {
  font-family: -apple-system, BlinkMacSystemFont, 'AppleSDGothicNeo-SemiBold', 'Sans-serif-medium','SFProDisplay','AppleGothicNeoSD', "NanumSquare", 'Flama';
}

a:active,
a:focus,
a:hover,
a {
  text-decoration: none;
  color:#000;
}

body {
  line-height: 1.5;
}

body,
p,
div,
h1,
h2,
h3,
h4,
ul,
ol,
li {
  padding: 0;
  margin: 0;
}

ul,
ol,li {
  list-style: none;
}

em {
  font-style: normal;
}

label.radio-skin {
  display: block;
}


button {
  padding: 0;
  margin: 0;
  line-height: 1.5;
  border:0;
}
