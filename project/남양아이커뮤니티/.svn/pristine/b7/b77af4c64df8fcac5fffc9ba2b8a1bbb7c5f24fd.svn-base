	<script>
		$(function () {
			// s : main-common-popup
			var options = {};
			var mainpopSlideLength = $('.mainpop-swiper-wrap .swiper-slide').length;
			if (mainpopSlideLength == 1) {
				options = {
					loop: false,
					simulateTouch: false
				}
				$('.mainpop-swiper-wrap .swiper-button-prev, .mainpop-swiper-wrap .swiper-button-next').hide();
			} else {
				options = {
					navigation: {
						nextEl: '.mainpop-swiper-wrap .swiper-button-next',
						prevEl: '.mainpop-swiper-wrap .swiper-button-prev',
					},
					pagination: {
						el: '.mainpop-swiper-wrap .swiper-pagination',
						clickable: true,
					},
					slidesPerView: 'auto',
					simulateTouch: false,
					spaceBetween: 15,
					speed: 500,
					loop: true,
				}
			}
			var mainPopSwiper = new Swiper('.mainpop-swiper-wrap .mainpop-swiper', options);

			$('.mainpop-swiper-wrap .swiper-slide').each(function (index) {
				if ($(this).find('.btn-box').length == 0) {
					$(this).addClass('none-btn');
				}
				if ($(this).find('.mainpop-btm-area').length == 0) {
					$(this).addClass('none-txt');
				}
				if ($(this).find('.img').length == 0) {
					$(this).addClass('none-img');
				}
			});
			// e : main-common-popup
		});
	</script>
