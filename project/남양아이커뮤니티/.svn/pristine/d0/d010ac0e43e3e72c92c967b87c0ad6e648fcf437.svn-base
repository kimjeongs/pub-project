	v2.0 | 20110126
	License:none (public domain)
*/

@import url('lib/swiper.min.css');
@import url('content.css');
@import url('main.css');
@import url('popup.css');
@import url('customlib.css');
@import url('pregnancy.css');
@import url('empathy.css');
@import url('care.css');
@import url('myroom.css');
@import url('growth.css');
@import url('calendar.css');
@import url('customer.css');
@import url('storybook.css');

@font-face {
  font-family:'NanumSquare';
  font-style:normal;
  font-weight:400;
  src:url(../fonts/NanumSquareR.eot);
  src:local('Nanum Square Regular'),
			local('NanumSquareR'),
			url(../fonts/NanumSquareR.eot?#iefix) format('embedded-opentype'),
			url(../fonts/NanumSquareR.woff2) format('woff2'),
			url(../fonts/NanumSquareR.woff) format('woff'),
			url(../fonts/NanumSquareR.ttf) format('truetype');
}

@font-face {
  font-family:'NanumSquare';
  font-style:bold;
  font-weight:700;
  src:url(../fonts/NanumSquareB.eot);
  src:local('Nanum Square Bold'),
			local('NanumSquareB'),
			url(../fonts/NanumSquareB.eot?#iefix) format('embedded-opentype'),
			url(../fonts/NanumSquareB.woff2) format('woff2'),
			url(../fonts/NanumSquareB.woff) format('woff'),
			url(../fonts/NanumSquareB.ttf) format('truetype');
}

/* base */
article, aside, details, figcaption, figure, footer, header, hgroup, nav, section {
	display:block;
}
audio, canvas, video {
	display:inline-block;
	*display:inline;
	*zoom:1;
}
audio:not([controls]) {
	display:none;
}
[hidden] {
	display:none;
}

mark {
	color:#5d5d5d;
	background:#ff0;
}
pre {
	white-space:pre;
	white-space:pre-wrap;
	word-wrap:break-word;
}
q, blockquote {
	quotes:none;
}
q:before,
q:after,
blockquote:before,
blockquote:after {
	content:'';
	content:none;
}
address, em {
	font-style:normal;
}

sub, sup {
	position:relative;
	font-size:75%;
	line-height:0;
	vertical-align:baseline;
}
svg:not(:root) {
	overflow:hidden;
}
legend {
	border:0;
	*margin-left:-7px;
}
ul, ol, li {
	list-style:none;
}
button, input {
	border:0;
	-webkit-appearance:none;
	line-height:normal;
	*overflow:visible;
}
input::placeholder {color:#aaa;}
button,
html input[type='button'],
input[type='reset'],
input[type='password'],
input[type='number'],
input[type='email'],
input[type='address'],
input[type='tel'],
input[type='submit'] {
	color:#000;
	-webkit-appearance:button;
}
input[type=text]::-ms-clear {
  display:none;
}
input[type="number"]::-webkit-outer-spin-button,
input[type="number"]::-webkit-inner-spin-button {
	-moz-appearance:none;
	-webkit-appearance:none;
	appearance:none;
	margin:0;
}
input[type=number] {
    -moz-appearance:textfield;
}
input[type='password'] {
	font-family:Arial, Helvetica, sans-serif;
}
input[type="search"]::-webkit-search-decoration,
input[type="search"]::-webkit-search-cancel-button,
input[type="search"]::-webkit-search-results-button,
input[type="search"]::-webkit-search-results-decoration { display:none;}
button {
	background:none;
	border:none;
	padding:0;
	letter-spacing:-1px;
}
img, embed, object, video {
	max-width:100%;
	height:auto;
}
label,
input.button, input.submit , input.image,
button {cursor:pointer;}
textarea, pre {overflow:auto;}
input[type='search']::-webkit-search-decoration {
	-webkit-appearance:none;
}
input[type='search']::-ms-clear {
	display:none;
}
/* label {
	cursor:default;
} */
button::-moz-focus-inner, input::-moz-focus-inner {
	padding:0;
	border:0;
}
button, input, select, textarea {
	font-size:100%;
}
img, textarea, input {
    margin:0;
    vertical-align:top;
}
table {
	border-collapse:collapse;
	border-spacing:0;
	table-layout:fixed;
}
table th {
	font-weight:normal;
}
* {
	-webkit-text-size-adjust:none;
	box-sizing:border-box;
}
body, h1, h2, h3, h4, h5, h6, p, blockquote, ul, ol, dl, dd, figure, form, fieldset, pre {
	padding:0;
	margin:0;
	font-weight:400;
}
body{
	font-family:'NanumSquare', '맑은 고딕', 'Malgun Gothic', '돋움', 'gulim', '굴림', Helvetica, sans-serif;
	font-size:14px;
	font-weight:400;
	line-height:20px;
	color:#444;
	letter-spacing:-1px;
}
button, input, select, textarea {
	font-family:'NanumSquare', '맑은 고딕', 'Malgun Gothic', '돋움', 'gulim', '굴림', Helvetica, sans-serif;
}
pre,
code,
kbd,
samp {
	font-family:'NanumSquare', '맑은 고딕', 'Malgun Gothic', '돋움', 'gulim', '굴림', Helvetica, sans-serif;
	font-size:1em;
}
strong {
	font-weight:700;
}
fieldset {
	border:0;
}
hr {
	display:none;
}
img {
	border:0;
	-ms-interpolation-mode:bicubic;
	pointer-events:none;
}
a {
	display:inline-block;
	color:#000;
	text-decoration:none;
	vertical-align:top;
	letter-spacing:-1px;
}
a:hover,
a:hover span,
a:hover strong {
	text-decoration:none;
	outline:none;
}
textarea {
	width:100%;
	min-width:100%;
	max-width:100%;
	height:80px;
	padding:7px 8px;
	border:0;
	color:#000;
	line-height:16px;
	vertical-align:top;
	word-break:break-all;
	box-sizing:border-box;
	-webkit-box-sizing:border-box;
	-moz-box-sizing:border-box;
}
caption {
	overflow:hidden;
	width:0;
	height:0;
	font-size:0;
	line-height:0;
	visibility:hidden;
}
button:active span,
button:focus span {
	position:relative;
	left:0;
	top:0;
}

select {
	margin:0;
	padding:0;
	border:0;
	background:none;
	-webkit-appearance:none;
	appearance:none;
}
.content {
	position: relative;
	min-height:483px;
	padding-bottom:40px;
}
.content.search {
	padding-bottom:0;
}
.blind {
	position:absolute;
	clip:rect(0, 0, 0, 0);
	height:1px;
	margin-top:-1px;
}
.hidden {
	position: absolute;
	padding: 0;
	text-indent: -9999px;
}

/* input */
input::-webkit-input-placeholder {font-weight:400;}
input::placeholder {font-weight:400;}
input:-ms-input-placeholder {font-weight:400;}
.inp-box {margin:0;padding:0;border:0;background:none;}
input[type="text"].error {border:1px solid #ec1f2f;}
.ipt-txt {width: 100%; height:40px;padding:10px;border:1px solid #ddd;border-radius:7px;font-size:13px;line-height:18px;}
.ipt-txt-box .ipt-txt:disabled {background-color: #eee;border-color: #ddd !important; color:#ccc;}
.ipt-txt-box .ipt-txt:focus,
.ipt-txt-box .ipt-txt:hover,
.ipt-txt-box .ipt-txt:active {border-color: #000;}
.ipt-txt-btn {position:relative;width:100%;padding-right:120px;}
.ipt-txt-btn .ipt-txt {width:100%;}
.ipt-txt-btn .btn-box {position:absolute;right:0;top:50%;-webkit-transform:translateY(-50%);-moz-transform:translateY(-50%);-ms-transform:translateY(-50%);transform:translateY(-50%);}
.ipt-txt-btn .btn-box button {margin:0;}

/* select */
.inp-select-box select {width:100%;border:1px solid #ddd;height:40px;border-radius:5px;padding-left:9px;appearance:none;-webkit-appearance:none;background:#fff url(../images/content/icon/ico_down_arrow.png) no-repeat right center; background-size: 20px 6px;  font-size:13px;color:#000;}
.inp-select-box select:focus,
.inp-select-box select:active {color: #000;border-color: #000;background-image: url(../images/content/icon/ico_down_arrow_on.png)}
.inp-select-box select:disabled {background: #eee url(../images/content/icon/ico_down_arrow_disabled.png) no-repeat right center;border-color: #ddd; background-size:22px; color: #aaa}
.inp-select-box + .inp-select-box,
.inp-select-box + .double-select,
.double-select +  .inp-select-box,
.inp-select-box + .ipt-txt-box { margin-top: 10px;}
.double-select {overflow: hidden;margin: 0 -4px; }
.double-select .inp-select-box,
.double-select .ipt-txt-box {float:left;width:50%;margin-top:0;padding: 0 4px;}
.double-select .inp-select-box:first-child,
.double-select .ipt-txt-box:first-child {float: left;}
.double-select .inp-select-box:last-child,
.double-select .ipt-txt-box:last-child{float: right;}
.form-slt-box .slt-box {width:100%;padding:11px 15px;background:#f5f5f5 url(../images/common/icon/ico_slt_arrow.png) no-repeat right center;background-size:26px auto;font-size:12px;color:#000;line-height:23px;letter-spacing:-1px;}
/* checkbox */
.inp-chk {display:none;}
.inp-chk + label {position:relative;display:inline-block;padding-left:33px;font-size:13px;line-height:24px;color:#444;letter-spacing:-1.5px;cursor:pointer;}
.inp-chk + label:before {content:'';position:absolute;top:13px;left:0;width:23px;height:23px;margin-top:-13px;border:1px solid #ccc;border-radius:5px;background:#fff;}
.inp-chk + label:after {content:'';position:absolute;top:13px;left:5px;width:13px;height:8px;margin-top:-4px;background:url(../images/common/icon/ico_chk_off.png) no-repeat center center;background-size:12px 8px;z-index:2;}
.inp-chk:checked+label{color: #000;}
.inp-chk:checked + label:before {border-color:#000;}
.inp-chk:checked + label:after {background-image:url(../images/common/icon/ico_chk_on.png);}
.inp-chk:disabled + label:after{background-image:url(../images/common/icon/ico_chk_disabled.png);}
.inp-chk:disabled + label {cursor:auto;}
.inp-chk:checked:disabled + label:before{border:1px solid #ddd;}
.inp-chk:checked:disabled + label:after{background-image:url(../images/common/icon/ico_chk_on_disabled.png);}
.inp-chk + label.hidden {width:0;padding-left:25px;font-size:0;/* line-height:0; */}

/* radio */
.ipt-rdo {display:none;}
.ipt-rdo + label {display:inline-block;position:relative;padding-left:30px;font-size:14px;line-height:26px;color:#000;}
.ipt-rdo + label:before {content:'';position:absolute;top:50%;left:0;width:25px;height:26px;margin-top:-12px;background:url(../images/common/icon/ico_radio.png) no-repeat 0 -25px; background-size: 25px;}
.ipt-rdo:checked + label:before {background-position:0 0;}
.ipt-rdo:disabled + label {cursor:auto;}
.ipt-rdo:disabled + label:before {background-position:0 -51px;}
.ipt-rdo:checked:disabled + label:before {background-position:0 -76px;}
.radio-set li {display:inline-block;margin-left:10px;}
.radio-set li:first-child {margin-left:0;}

/* search box */
.item-search{position:relative;}
.item-search + .item-search{margin-top: 20px;}
.item-search .ipt-search{border:1px solid #ddd;border-radius:7px;padding:10px 40px 10px 10px;width:100%;height:40px;font-size: 13px;}
.item-search .ipt-search:focus,
.item-search .ipt-search:active{ border-color: #000; color: #000;}
.item-search .ipt-search:disabled{border-color: #ddd;background: #eee;}
.item-search .ipt-search:disabled + .btn-search {background-position: 10px -35px;}
.item-search .btn-search{display:block;position:absolute;right:0;top:1px;width:40px;height:38px;white-space:nowrap;text-indent:101%;overflow:hidden;background:url(../images/common/icon/ico_cont_search.png) no-repeat 10px 10px; background-size: 16px;}

/* date box */
.date-box{display: inline-block; width: 100%;text-align: center;}
.date-box .date{width:48%;}
.date-box .date{float:left;}
.date-box .range{display:block;float:left;width:4%;line-height:40px;}

/* Button1 */
.btn-box {margin-top: 30px; text-align:center;}
.btn-box button + button,
.btn-box a + a,
.btn-box button + a,
.btn-box a + button {margin-left:2px;}
.btn-type1 {position:relative;min-width:150px;height:45px;padding:0 20px;background-color:#fff;border:1px solid #000;border-radius:30px;}
.btn-type1 span {font-size:15px;font-weight:700;color:#000;line-height:43px;vertical-align:middle;}
.btn-type1:active{background:#f6f6f6;}
.btn-type2 {position:relative;min-width:150px;height:45px;padding:0 20px;background-color:#fff;border:1px solid #ec1f2f;border-radius:30px;}
.btn-type2 span {font-size:15px;font-weight:700;color:#ec1f2f;line-height:43px;vertical-align:middle;}
.btn-type2:active{background:#f6f6f6;}
.btn-type3 {position:relative;min-width:150px;height:45px;padding:0 20px;background-color:#333;border-radius:30px;}
.btn-type3 span {font-size:15px;font-weight:700;color:#fff;line-height:43px;vertical-align:middle;}
.btn-type3:active {background-color:#222;}
.btn-type3.vote-complete:active {background-color:#333;}
.btn-type3.vote-complete span {position:relative;padding-left:24px;}
.btn-type3.vote-complete span:before {content:'';position:absolute;top:50%;left:0;width:14px;height:9px;margin-top:-5px;background-image:url(../images/common/icon/ico_vote_check.png);}
.btn-type4 {position:relative;min-width:150px;height:45px;padding:0 20px;background-color:#ec1f2f;border-radius:30px;}
.btn-type4 span {font-size:15px;font-weight:700;color:#fff;vertical-align:middle;line-height:43px;}
.btn-type4:active {background-color:#e11c2c;}
.btn-type1:disabled,
.btn-type2:disabled,
.btn-type3:disabled,
.btn-type4:disabled {opacity:0.5;filter:alpha(opacity=50);cursor:auto;}

/* Button2 */
.btn-medium {position:relative;height:40px;padding:0 15px;background-color:#ec1f2f;border-radius:30px;}
.btn-medium span {font-size:14px;color:#fff;vertical-align:middle;line-height:40px; font-weight: 400;}
.btn-medium:active {background-color:#e11c2c;}
.btn-medium:disabled {opacity:0.5;filter:alpha(opacity=50);cursor:auto;}
.btn-m-squre{min-width: auto; height: 40px; padding: 0 15px; border-radius: 7px;}
.btn-m-squre span{font-size: 14px; line-height: 40px;font-weight: 400;}
.btn-m-squre.btn-add span{padding-left: 20px; background: url('../images/common/icon/btn_add.png') no-repeat left center; background-size: 13px;}
.btn-m-squre.btn-chk span{padding-left: 20px; background: url('../images/common/icon/btn_sm_select.png') no-repeat left center; background-size: 13px;}
.btn-medium.fix-fill { width: 125px;}
.btn-medium.fix-fill span { font-weight: 700;}
.btn-medium.fix-line {width:125px;background-color:#fff;border:1px solid #ec1f2f;}
.btn-medium.fix-line span {color:#ec1f2f; }
.btn-medium.fix-line:active{background:#f6f6f6;}

/* Button3 */
.btn-s-squre {min-width: auto; height: 35px; padding: 0 15px; border-radius: 7px;}
.btn-s-squre span {font-size: 12px; line-height: 35px;font-weight: 400;}
.btn-s-squre.select span {padding-left: 17px;background: url('../images/common/icon/btn_sm_select.png') no-repeat left center;background-size: 11px 8px; }

/* Button4 */
.btn-type1.small {min-width:72px;height:30px;padding:0 10px;border:1px solid #000;border-radius:7px;}
.btn-type2.small,
.btn-type3.small,
.btn-type3.small,
.btn-type4.small {min-width:72px;height:30px;padding:0 10px;border-radius:7px;}
.btn-type1.small span,
.btn-type2.small span,
.btn-type3.small span,
.btn-type4.small span {font-size:12px;font-weight:400;line-height:18px;}
.btn-type3.small:disabled span {color: #d0d0d0;}
.btn-type1:before,
.btn-type2:before,
.btn-type3:before,
.btn-type4:before {content:'';display:inline-block;height:100%;vertical-align:middle;}

.btn-common-link {position:relative;padding-right:30px;font-size:13px;color:#000;line-height:23px;letter-spacing:-1.5px;}
.btn-common-link:after {content:'';position:absolute;top:50%;right:0;width:23px;height:23px;margin-top:-12px;background-image:url(../images/common/icon/ico_btn_common_link.png);background-size:23px 23px;}

/* title, text */
.sub-title {display: block; margin-bottom: 18px; font-size:18px;line-height:20px;font-weight:700;color:#000;letter-spacing:-0.5px;}
.c-title {display: inline-block; padding-left: 15px; font-size:18px;font-weight:700;color:#000;letter-spacing:-0.5px;}
.board-info .s-title{font-size:22px;line-height:24px;font-weight:700;color:#000;}
.info-sub-text{font-size: 11px; color:#444;}

/* list style */
.list-dash {margin-top:15px;}
.list-dash li {position: relative; padding-left: 10px; color: #7e7e7e;font-size: 11px;}
.list-dash li:before{position: absolute; top: 9px;left: 0; display: block; width: 5px; height: 1px; background: #7e7e7e;content: '';}
.list-dot li {position: relative; padding-left: 10px; color: #444;font-size: 11px; line-height: 16px;}
.list-dot li+li {margin-top: 5px;}
.list-dot li:before{position: absolute; top: 9px;left: 0; display: block; width: 2px; height: 2px; background: #444; border-radius: 25px; content: '';}

/* pagination */
.cont-list-pagination {position:relative;overflow:hidden;max-width:360px;margin:26px auto 0;padding:0 57px;font-size:0;text-align:center;}
.cont-list-pagination > a,
.cont-list-pagination > span {display:inline-block;width:29px; height: 29px; vertical-align:top; border: 1px solid #ddd;}
.cont-list-pagination .pagination-left-btn {position:absolute;top:0;left:15px;}
.cont-list-pagination .pagination-right-btn {position:absolute;top:0;right:15px;}
.cont-list-pagination .pagination-left-btn a,
.cont-list-pagination .pagination-right-btn a,
.cont-list-pagination .pagination-left-btn span,
.cont-list-pagination .pagination-right-btn span {display:inline-block;width:29px; height: 29px; vertical-align:top; border: 1px solid #ddd;}
.cont-list-pagination .prev,
.cont-list-pagination .last{margin-left: -1px;}
.pagination-list {display:inline-block;width:100%;font-size:0;vertical-align:top;}
.pagination-list li {display:inline-block;width:30px;height:29px;vertical-align:top;}
.pagination-list li+li {margin-left: -1px;}
.pagination-list li a{border:1px solid #ddd;}
.pagination-list li a,
.pagination-list li span {display: inline-block; width: 100%;height: 100%;font-size:12px;color:#999;line-height:30px;letter-spacing:0.5px;vertical-align:top;}
.pagination-list li span {position: relative; color:#000; border:1px solid #000; cursor:default;z-index: 1;}

/* 스위치 */
.circle-switch-box span {position:relative;display:inline-block;;width:40px;height:24px;border-radius:50px;background:#aaa;font-size:0;line-height:0;vertical-align:middle;cursor:pointer; margin-top: -2px;}
.circle-switch-box.on span {background:#ec1f2f;}
.circle-switch-box span:before {content:'';position:absolute;top:9px;left:0;-webkit-transition:all .2s ease-in-out;-moz-transition:all .2s ease-in-out;-ms-transition:all .2s ease-in-out;transition:all .2s ease-in-out;width:20px;height:20px;margin-top:-9px;border:2px solid #aaa;border-radius:50%;background:#fff;}
.circle-switch-box.on span:before {left:16px;-webkit-transition:all .2s ease-in-out;-moz-transition:all .2s ease-in-out;-ms-transition:all .2s ease-in-out;transition:all .2s ease-in-out;border-color:#ec1f2f;}

/* tab */
.tab-cont .tab-item {display:none;}
.tab-cont .tab-item.active {display:block;}

/* notice */
.information-box {width:100%;padding:25px 20px 30px;border-radius:7px;background:#f6f6f6;}
.information-box .information-list-title {width:162px;height: 22px;padding-left:30px;margin-bottom: 10px;background:url(../images/common/icon/ico_happy_box_caution.png) no-repeat left top;font-size:15px;font-weight:700;color:#000;line-height:19px;letter-spacing:-0.5px;vertical-align:top;}
.information-box strong { display: block; font-size: 12px;line-height: 37px; }

/* dev margin class */
.mt60 {margin-top:60px !important;}

/** common **/
/* layout */

/* header */
.header-cont {position:fixed;top:0;left:0;right:0;z-index:15;padding:0 11px;border-bottom:1px solid #ddd;background:rgba(255,255,255,.95);}
.header-cont:after {content:'';display:block;clear:both;}
.header-sub .i-wrapper-sub,
.depth-box .i-wrapper {overflow:hidden;}
.header-sub .i-wrapper-sub {border-bottom:1px solid #ddd;}
.header-cont .btn-gnb-main {padding:16px 4px 15px;}
.header-cont .btn-gnb-main span {display:block;width:75px;}
.header-cont .btn-gnb-prev {float:left;padding:16px 5px 15px 4px;}
.header-cont .btn-gnb-prev .ico {overflow:hidden;display:inline-block;width:11px;height:19px;}
.header-cont .btn-gnb-prev .ico img {width:11px;}
.header-cont .btn-gnb-prev:active .ico img {margin-top:-19px;}
.header-cont .btn-gnb-select {float:left;}
.header-cont .btn-gnb-select span {position:relative;display:inline-block;padding:14px 34px 14px 7px;font-size:16px;font-weight:700;color:#000;line-height:22px;}
.header-cont .btn-gnb-select.toggle span:after {content:'';position:absolute;top:50%;right:8px;width:19px;height:19px;margin-top:-10px;background:url(../images/header/icon/ico_gnb_select.png) no-repeat left top;background-size:19px auto;}
.header-cont .btn-gnb-select.toggle:active span:after {background-position:left -19px;}
.header-cont .btn-gnb-select.toggle.on span:after {background-image:url(../images/header/icon/ico_gnb_select_on.png);}
.header-cont .btn-gnb-select.toggle.on:active span:after {background-position:left -19px;}
.gnb-right-box {position:relative;float:right;margin-right:-3px;}
