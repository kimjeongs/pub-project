.smart-calendar-plan .plan-box .more {position:absolute;right:14px;bottom:14px;display:block;width:18px;height:18px;background:#ec1f2f;color:#fff;font-size:11px;text-align:center;line-height:18px;border-radius:50px;letter-spacing:0;}
