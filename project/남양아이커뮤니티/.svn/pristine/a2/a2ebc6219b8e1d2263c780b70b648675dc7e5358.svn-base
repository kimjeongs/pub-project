	<script type="text/javascript">
		$(function () {
			myScrolls = new IScroll('.i-wrapper-sub', {
				eventPassthrough: true,
				scrollX: true,
				scrollY: false,
				preventDefault: false,
				click: true,
				tap: true
			});
			myScroll = new IScroll('.i-wrapper', {
				eventPassthrough: true,
				scrollX: true,
				scrollY: false,
				preventDefault: false,
				click: true,
				tap: true
			});

			$('.gnb-depth3 li').click(function () {
				var depth3Idx = $(this).index() + 1;
				$(this).find('a').attr('href',
					'javascript: myScrolls.scrollToElement(document.querySelector(".i-scroll li:nth-child(' + depth3Idx +
					')"), null, -15, true)');
				setTimeout(function () {
					$('.i-scroll li:nth-child(' + depth3Idx + ')').find('a').attr('href', "#");
				});
			});

			$('.gnb-depth4 li, .gnb-right-depth li').click(function () {
				var depth4Idx = $(this).index() + 1;
				$(this).find('a').attr('href',
					'javascript: myScroll.scrollToElement(document.querySelector(".i-scroll-sub li:nth-child(' +
					depth4Idx + ')"), null, -15, true)');
				setTimeout(function () {
					$('.i-scroll-sub li:nth-child(' + depth4Idx + ')').find('a').attr('href', "#");
				});
			});
		});
	</script>
