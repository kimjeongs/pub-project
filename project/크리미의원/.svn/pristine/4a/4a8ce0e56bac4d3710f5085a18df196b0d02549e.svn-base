.pc-only {display:block !important;}
.mobile-only {display:none !important;}
#visualSlide { position:relative;}
#visualSlide .aritcle img { width:100%; vertical-align:top;}
#visualSlide li {
  display:none;position:relative; width:100%; overflow:hidden;
}
#visualSlide li.on {display:block;}
#visualSlide .obj-play {
  position:absolute;
  width:100%;
  top:calc(50% - 110px);
  text-align:center;
}
#visualSlide .obj-play p {
  color:#fff;
  font-size:61px;
  line-height: 70px;
  line-height:1;
  font-weight: 900;
}
#visualSlide .obj-play button { background:none; margin-top:20px; cursor: pointer;}

#visualSlide .controls .slide-prev { position:absolute; bottom:50px; left:50px;}
#visualSlide .controls .slide-next { position:absolute; top:50px; right:50px;}
.section1 .banner {
  width:100%;
  height:228px;
  overflow:hidden;
}
.section1 .banner li {
  float:left;
  width:50%;
}
.section1 .banner li a {
  display:block;
  width:100%;
  height:228px;
  color:#fff;
  padding:48px 0 0 61px;
  box-sizing: border-box;
}

.section1 .banner a em {
  display:block;
  line-height:1.38;
  font-size:18px;
}
.section1 .banner a strong {
  display:block;
  line-height:1.38;
  font-size:34px;
  font-weight: 900;
}
.section1 .banner a span {
  position:relative;
  display:block;
  padding-top:38px;
  margin-top:15px;
  line-height:1.38;
  font-size:14px;
  font-weight: 700;
}
.section1 .banner a span:after {
  content:'';
  position:absolute;
  left:0;
  top:0;
  width:20px;
  border-top:1px solid #f7b3be;
}
.section2 {
  width:100%;
  overflow:hidden;
  background:#f1f1ff;
  padding:0 0 100px;
  box-sizing: border-box;
}
.section2 .swiper-pagination { opacity:0;}

.section2  .ctit-wrap {
  width: 1465px;
  padding: 85px 0 25px;
  margin:0 auto;
  overflow: hidden;
}
.section3 .ctit-wrap {
  width:100%;
  overflow:hidden;
  margin-top:85px;
}
.section3 .ctit-wrap h2,
.section2 .ctit-wrap h2 {
  float:left;
  font-size:36px;
  margin-right:17px;
  color:#333;
}
.section3 .ctit-wrap span,
.section2 .ctit-wrap span {
  float:left;
  font-size:18px;
  margin-top:24px;
  color:#333;
}
.section2 .swipe-area {
  position:relative;
  width: 1465px;
  margin: 0 auto;
  background: #fff;
}
.section2 .swipe-area li { width:100%; overflow:hidden;}
.section2 .swipe-area li .thumb-cont {
  float:left;
  width:1074px;
  height: 520px;
}
.section2 .swipe-area li .thumb-cont img {
  width:100%;
  height: 100%;
  vertical-align:top;
}
.section2 .swipe-area .info-cont {
  float:left;
  width:calc(100% - 1074px);
  padding:0 64px 0 40px;
  box-sizing: border-box;
  letter-spacing: -1px;
}
.section2 .swipe-area .info-cont .goods-title {
  margin-top:74px;
}
.section2 .swipe-area .info-cont .goods-title > span { display:block; font-size:22px; line-height:1.5; color:#232323;}
.section2 .swipe-area .info-cont .goods-title > strong { display:block; margin-top: 4px; font-size:38px; line-height:1.5; color:#333333;}
.section2 .swipe-area .info-cont .goods-detail > span {
  position:relative;
  display:block;
  margin-top:87px;
  padding-top:27px;
  padding-bottom:5px;
  font-size:21px;
  line-height:1.5;
  color:#6c97d5;
}
.section2 .swipe-area .info-cont .goods-detail > span:after {
  content:'';
  position:absolute;
  top:0;
  left:0;
  width:108px;
  border-top:1px solid #bdbdc0;
}
.section2 .swipe-area .info-cont .goods-detail > em {
  position:relative;
  font-size:32px;
  color:#333;
  line-height:1.5;
  letter-spacing: -1px;
}
.section2 .swipe-area .info-cont .goods-detail > em:after {
  content:'';
  position:absolute;
  left:0;
  top:14px;
  width:100%;
  border-top:1px solid #333;
}
.section2 .swipe-area .info-cont .goods-detail > strong {
  font-size:40px;
  color:#333;
  line-height:1.5;
  letter-spacing: -1px;
  margin-left: 20px;
}
.section2 .swipe-area .info-cont .goods-detail > strong .unit {
  font-size: 34px;
}
.section2 .swipe-area .controls {
  position:absolute;
  right:0;
  bottom:10px;
  height:70px;
  width:calc(100% - 1074px);
}
.section2 .swipe-area .controls  .slide-prev {
  position:absolute;
  top:27px;
  left:50px;
  width: 28px;
  height: 19px;
  background: url(../img/common/ico_arrow.png) no-repeat;
  text-indent: -9999px;
}
.section2 .swipe-area .controls  .slide-next {
  position:absolute;
  top:27px;
  right:50px;
  width: 28px;
  height: 19px;
  background: url(../img/common/ico_arrow.png) no-repeat;
  transform: rotate(180deg);
  text-indent: -9999px;
}
.section2 .more-wrap {
  padding-top:40px;
  overflow:hidden;
  width: 1465px;
  margin: 0 auto;
}
.section2 .more-wrap .ctab {
  float:left;
  width:50%;
  overflow:hidden;
}
.section2 .more-wrap .ctab li { position:relative; float:left;}
.section2 .more-wrap .ctab li a {
  display:block;
  padding:0 48px;
  color:#68686e;
  font-size:18px;
}
.section2 .more-wrap .ctab li:first-child a {
  padding-left: 0;
}
.section2 .more-wrap .ctab li:after {
  content:'';
  position:absolute;
  left:0;
  top:5px;
  height:17px;
  border-left:1px solid #c9c9d6;
}
.section2 .more-wrap .ctab li:first-child:after {
  display:none;
}
.section2 .more-wrap .ctab li:first-child { padding-left:0;}
.section2 .more-wrap .ctab li.on a { color:#59595d; font-weight:bold;}
.more-wrap a.more {
  float:right;
  min-width: 200px;
  height:38px;
  border:1px solid #bcbcc7;
  border-radius:19px;
  padding:9px 26px 0;
  line-height:1.38;
  font-size:15px;
  color:#333;
  letter-spacing: -1px;
  box-sizing: border-box;
  background: url(../img/common/ico_btn_arrow.png) no-repeat 145px 18px;
}
#container > div { margin-top:20px;}
#container > div.section1 { margin-top:0;}

.section3 {
  width:100%;
  padding-bottom: 60px;
  overflow:hidden;
}
.section3 .more-wrap {
  width:1463px;
  margin: 0 auto;
  overflow:hidden;
}
.section3 .more-wrap .ctit-wrap { float:left; width:50%; margin-bottom:30px;}
.section3 .more-wrap a.more {
  float:right;
  height:38px;
  border:1px solid #bcbcc7;
  border-radius:19px;
  margin-top:92px;
  padding:9px 26px 0;
  line-height:1.38;
  font-size:15px;
  color:#333;
  box-sizing: border-box;
}

.section3 .thumb-list {
  position: relative;
  display:flex;
  width:1463px;
  margin: 0 auto;
  height:670px;
}
.section3 .thumb-list:after{
  position: absolute;
  left: -50px;
  bottom: -60px;
  content: '';
  width: 576px;
  height: 400px;
  background:#f2f2ff;
  z-index: -1;
}

.section3 .thumb-list .main-item {
  flex:1;
  /* max-width:485px; */
}
.section3 .thumb-list .thumb-item {
  position:relative;
  width:100%;
  height:50%;
  overflow:hidden;
}
.section3 .thumb-list .main-item .thumb-item {
  height:100%;
}
.section3 .thumb-list .main-item .thumb-item .thumb {
  display: block;
  width: 100%;
  height: 490px;
}
.section3 .thumb-list .main-item .thumb-item > p {
  position: relative;
  background: #fff;
  height: 180px;
  padding: 25px 0 0 35px;
  box-sizing: border-box;
}
.section3 .thumb-list .main-item .thumb-item > p:before {
  content: '';
  position: absolute;
  top: -5px;
  left: 0;
  width: 175px;
  height: 5px;
  background: #8a8ae5;
}
.section3 .thumb-list .main-item .thumb-item > p strong {
  display: block;
  font-size: 27px;
  font-weight: 400;
  line-height: 35px;
  letter-spacing: -1px;
  color: #333;
}
.section3 .thumb-list .main-item .thumb-item > p .price {
  display: block;
  font-size: 27px;
  font-weight: 900;
  line-height: 35px;
  letter-spacing: -1px;
  color: #333;
}
.section3 .thumb-list .main-item .thumb-item > p span{
  display: block;
  margin-top: 25px;
  font-size: 18px;
  letter-spacing: -1px;
  color:#8a8ae5;
}
.section3 .thumb-list .main-item .thumb img {
  width:100%;
  vertical-align:top;
}

.section3 .thumb-list .thumb-item .icon {
  position:absolute;
  top:20px;
  left:20px;
  width:53px;
  height:53px;
  border-radius:60px;
  background:#a9a9e7;
  color:#fff;
  text-align:center;
  padding-top:12px;
  font-size:20px;
  box-sizing: border-box;
  z-index: 1;
}
.section3 .thumb-list .bind {
  flex:1;
  max-width: 488px;
  border-left: 1px solid #e6e6e6;
  border-bottom: 1px solid #e6e6e6;
}
.section3 .thumb-list .bind:last-child {
  border-right: 1px solid #e6e6e6;
}
.section3 .thumb-list .bind .thumb-item{
  padding: 20px 0 0 20px;
  border-top: 1px solid #e6e6e6;
  background: #fff;
  box-sizing: border-box;
}
.section3 .thumb-list .bind .thumb-item a{
	display:block;
	height:100%;
}
.section3 .thumb-list .bind .thumb-item .thumb {
  position:absolute;
  right:0;
  bottom:0;
  z-index:1;
}
.section3 .thumb-list .bind .thumb-item .thumb img { width:100%; vertical-align:top;}
.section3 .thumb-list .bind .thumb-item p {
  position:absolute;
  left:30px;
  top:175px;
  z-index:10;
}
.section3 .thumb-list .bind .thumb-item p strong {
  display: block;
  font-size: 27px;
  font-weight: 400;
  line-height: 35px;
  color: #333;
  letter-spacing: -1px;
}
.section3 .thumb-list .bind .thumb-item p .price {
  display: block;
  font-size: 27px;
  font-weight: 900;
  line-height: 35px;
  color: #333;
  letter-spacing: -1px;
}
.section3 .thumb-list .bind .thumb-item p span {
  display: block;
  margin-top: 23px;
  font-size: 18px;
  font-weight: 400;
  line-height: 35px;
  color: #8a8ae5;
  letter-spacing: -1px;
}

#container .section4 {
