.top-banner {
	background:#d6e3d2;
	text-align:center;
}
.top-banner a {
	display: inline-block;
	margin-right: 230px;
}
.top-banner img { vertical-align:top;}
.top-banner .btn-close {
    width: 45px;
    height: 45px;
    background: url(../img/common/btn_top_banner_close.png) no-repeat;
    text-indent: -9999px;
    vertical-align: top;
    margin-top: 10px;
    cursor: pointer;
}
#header {
	display:flex;
	width:100%;
	max-width:1832px;
	margin: 0 auto;
	overflow:hidden;
}

#header h1 {
	flex:1;
	flex-basis: 368px;
	text-align:right;
	padding:36px 82px 0 0;
}
#header .dimm-wrap {

}
#gnb {
	flex:300;
}
#gnb ul {
	float: left;
}
#gnb li {
	float:left;

}
#gnb li a {
	display:block;
	height:117px;
	padding:50px 40px 0 40px;
	box-sizing: border-box;
	font-size: 19px;
	font-weight: 700;
	color: #000;
}
#gnb .quick-link {
	float:right;
	margin-top:54px;
}
#gnb .quick-link a {
	position:relative;
	float:left;
	padding:0 20px;
	font-size: 16px;
	font-weight: 700;
	color: #999;
}
#gnb .quick-link a:first-child:after {
	content:'';
	position:absolute;
	right:0;
	top:5px;
	height:14px;
	border-right:1px solid #b8b8b8;
}
#gnb .quick-link select {
	min-width:140px;
	height: 40px;
	padding: 0 30px;
	margin-left:63px;
	margin-top:-50px;
	background: #8a8ae5 url(../img/common/ico_select.png) no-repeat 110px center;
	border-radius: 50px;
	color: #fff;
	font-size: 18px;
	line-height: 40px;
	letter-spacing: -1px;
	font-weight: 700;
	-webkit-appearance: none;
	-moz-appearance: none;
	 appearance: none;
}
#m-gnb,
.m-menu,
.btn-search {display:none;}

#header .site_link {
    position: relative;
}

#header .site_link > a {
    display: block;
    width: 100px;
    height: 35px;
    border-radius: 100px;
    background: #8a8ae5;
    color: #fff;
    line-height: 35px;
    float: left;
    position: relative;
    margin-top: 20px;
    text-indent: -25px;
}

#header .site_link > a::after,
#header .site_link > a::before {
    content: '';
    position: absolute;
    top: 17px;
    width: 10px;
    height: 1px;
    background: #fff;
}

#header .site_link > a::after {
    right: 20px;
    transform: rotate(45deg);
}

#header .site_link > a::before {
    right: 13.5px;
    transform: rotate(-45deg);
}

#header .site_link ul {
    position: absolute;
    top: 101%;
    left: 0;
    width: 100px;
    height: 100px;
    border-radius: 5px;
    background: #8a8ae5;
    overflow: hidden;
    overflow-y: auto;
    display: none;
}

#header .site_link ul li {
    float: none;
}

.style-1::-webkit-scrollbar-track {
    background: #8a8ae5;
    border-radius: 5px 5px 0 0;
    overflow: hidden;
}

.style-1::-webkit-scrollbar {
    width: 8px;
    background-color: #F5F5F5;
    border-radius: 5px 5px 0 0;
    overflow: hidden;
}

.style-1::-webkit-scrollbar-thumb {
    background-color: #a2a2de;
    border-radius: 5px;
}

#header .site_link ul a {
    display: block;
    color: #fff;
    line-height: 35px;
}

#header .site_link ul a:hover {
    background: #4949af;
}

/*컨텐츠*/
#container {
    background: #fff;
    overflow: hidden;
}
