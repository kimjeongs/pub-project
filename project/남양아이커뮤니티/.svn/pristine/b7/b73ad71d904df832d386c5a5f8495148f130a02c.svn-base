		$('.baby-pic-days .swiper-container').each(function (index, element) {
			var $this = $(this);
			options = {},
			embryoSlideLength = $(this).find('.swiper-slide').length;
			$this.addClass('embryo-month-' + index);
			if (embryoSlideLength == 1) {
				options = {
					allowSlidePrev: false,
					allowSlideNext: false,
					simulateTouch: false,
					loop: false,
					pagination: false,
				}
				$('.embryo-month-' + index + ' .swiper-pagination').hide();
			} else {
				options = {
					simulateTouch: true,
					speed: 500,
					loop: true,
					pagination: {
						el: $('.embryo-month-' + index + ' .swiper-pagination'),
						clickable: true,
					},
				}
			}
			var swiper1 = new Swiper('.embryo-month-' + index, options);
		});
	</script>
