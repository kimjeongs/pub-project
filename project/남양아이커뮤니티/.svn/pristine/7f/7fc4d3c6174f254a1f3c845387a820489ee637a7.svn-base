#jp_container_N {visibility:hidden;position:relative;overflow:hidden;padding:0;border:0;box-shadow:0 5px 20px 0 rgba(0,0,0, .1);}
.jp-audio, .jp-audio-stream, .jp-video {background:#fff;font-family: 'NanumSquare', '맑은 고딕', 'Malgun Gothic', '돋움', 'gulim', '굴림', Helvetica, sans-serif;font-size:14px;font-weight:400;color: #444;line-height:20px;letter-spacing:-1px;}


/* .jplayer-list-area {padding:0 10px;} */
.jplayer-title {padding:15px;font-size:18px;font-weight:700;color:#fff;background-color:#333;line-height:22px;letter-spacing:-1.5px;}
.jp-details, .jp-playlist {border-color:#aaa;background:#fff;}
div.jp-type-playlist div.jp-playlist li.jp-playlist-current {padding-left:15px;list-style:none;}
div.jp-type-playlist div.jp-playlist a.jp-playlist-current, div.jp-type-playlist div.jp-playlist a:hover {color:#ec1f2f;}
div.jp-type-playlist div.jp-playlist li:last-child {padding:9px 0 9px 10px;}
div.jp-type-playlist div.jp-playlist a.jp-playlist-item-remove {position:absolute;top:50%;right:-25%;float:none;width:18px;height:18px;margin-top:-9px;background:url(../../images/jplayer/bg_list_close.png) no-repeat center center;background-size:11px 11px;font-size:0;line-height:0;}
div.jp-type-playlist div.jp-playlist a {overflow:hidden;max-height:36px;font-size:13px;color:#444;line-height:18px;letter-spacing:-1px;}
.jp-playlist ul li {padding:15px 0 15px 15px;}
.jp-playlist ul li div {position:relative;display:block;width:79%;}
.jp-playlist ul li .jp-artist {display:none;}

.jp-interface {padding:20px 15px 15px;background:#333;}

.jp-gui {position:absolute;bottom:0;left:0;right:0;}
.jp-playlist ul {position:absolute;top:52px;left:0;right:0;bottom:154px;padding:0;overflow-y:auto;}

.jp-progress {background:#585858;}
.jp-seek-bar {background:#585858;}
.jp-play-bar {background:#ec1f2f;}
.jp-video .jp-current-time,
.jp-video .jp-duration {margin:2px 0 0;font-size:13px;font-style:normal;color:#fff;line-height:24px;}

.jp-controls-holder {width:auto;padding-bottom:70px;}
.jp-video .jp-volume-controls {top:26px;left:0;}
.jp-mute,
.jp-mute:focus {background:url(../../images/jplayer/ico_volume.png) no-repeat left top;}
.jp-state-muted .jp-mute,
.jp-state-muted .jp-mute:focus {background:url(../../images/jplayer/ico_volume_mute.png) no-repeat left top;}
.jp-volume-bar {left:26px;width:55px;background:#4b4b4b;}
.jp-volume-bar-value {background:#efefef;}
.jp-volume-controls button.jp-volume-max {display:none !important;}

.jp-video .jp-type-playlist .jp-controls {display:block;float:none;position: absolute;top:2px;width:100%;margin-left:0;text-align:center;}
.jp-video .jp-type-playlist .jp-controls:after {display:block;clear:both;content:'';}
.jp-previous,
.jp-previous:focus {width:20px;height:20px;background:url(../../images/jplayer/ico_prev.png) no-repeat center center;background-size:15px 11px;}
.jp-previous:active {background-image:url(../../images/jplayer/ico_prev_active.png);}
.jp-play,
.jp-play:focus {width:55px;height:55px;margin-left:7%;background:url(../../images/jplayer/ico_play.png) no-repeat left top;background-size:55px 55px;}
.jp-state-playing .jp-play,
.jp-state-playing .jp-play:focus {width:55px;height:55px;margin-left:7%;background:url(../../images/jplayer/ico_pause.png) no-repeat left top;background-size:55px 55px;}
.jp-next,
.jp-next:focus {width:20px;height:20px;margin-left:7%;background:url(../../images/jplayer/ico_next.png) no-repeat center center;background-size:15px 11px;}
.jp-next:active {background-image:url(../../images/jplayer/ico_next_active.png);}
.jp-next, .jp-previous, .jp-stop {margin-top:22px;}
.jp-stop,
.jp-stop:focus {width:20px;height:20px;background:url(../../images/jplayer/ico_stop.png) no-repeat center center;background-size:11px 11px;}
.jp-stop:active {background-image:url(../../images/jplayer/ico_stop_active.png);}
.jp-repeat,
.jp-repeat:focus {width:21px;height:16px;background:url(../../images/jplayer/ico_repeat.png) no-repeat left top;background-size:19px 14px;}
.jp-toggles button {width:21px;height:16px;}
.jp-state-looped .jp-repeat,
.jp-state-looped .jp-repeat:focus {background:url(../../images/jplayer/ico_repeat_press.png) no-repeat left top;background-size:19px 14px;}
.jp-video .jp-toggles {right:3%;width:auto;margin-top:27px;}
.jp-controls button:first-child {margin-left:30px;}

#jp_container_N .btn-popup-close {background:url(../../images/common/icon/ico_sticky_close.png) no-repeat center center;background-size:15px 15px;}