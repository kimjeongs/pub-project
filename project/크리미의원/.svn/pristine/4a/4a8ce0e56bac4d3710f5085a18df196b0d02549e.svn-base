}
#container .section5 .map-info .float-wrap .in-cont dt {
  float: left;
  width: 92px;
  font-size: 16px;
  letter-spacing: -1px;
  line-height: 26px;
  color: #333;
}
#container .section5 .map-info .float-wrap .in-cont dd {
  float: left;
  font-size: 16px;
  letter-spacing: -1px;
  line-height: 26px;
  color: #333;
  margin-left: 0;
}
#container .section5 .map-info .float-wrap .in-cont .tel-num {
  font-size: 30px;
  font-weight: 900;
  color: #aa89bd;
  letter-spacing: -1px;
  line-height: 50px;
}
#container .section5 .map-info .float-wrap .in-cont span {
  display: block;
  font-size: 16px;
  line-height: 34px;
  letter-spacing: -1px;
  font-weight: 300;
  color: #333;
}


