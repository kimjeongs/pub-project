.smart-calendar-plan .plan-box .cont .list .list-my-schedule span:nth-child(2):before {background:#fadbc3;}
.smart-calendar-plan .plan-box .cont .list .list-my-schedule span:nth-child(3):before {background:#fcedae;}
.smart-calendar-plan .plan-box .bottom {display:inline-block;position:absolute;bottom:0;left:0;width:100%;height:34px;border-top:1px solid #ddd;font-size:12px;line-height:34px;color:#000;text-align:center;}
