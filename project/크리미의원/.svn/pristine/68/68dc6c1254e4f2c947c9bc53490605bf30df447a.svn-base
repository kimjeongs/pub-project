}
.sub-section4 .btn-wrap {margin-top:33px;}
.sub-section4 .btn-wrap .btn-kakao span {
	display:inline-block;
	padding-left:43px;
	background:url('../img/common/ico_kakao.png') no-repeat 0 11px;
}
.sub-section4 .btn-white {
	display:inline-block;
