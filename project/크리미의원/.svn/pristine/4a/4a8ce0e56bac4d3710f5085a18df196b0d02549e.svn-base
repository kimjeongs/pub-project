#container .section5 {
  height: 348px;
  margin-bottom: 20px;
  overflow: hidden;
}
#container .section5 .inner {
  display: flex;
}
#container .section5 .map {
  flex: 1;
  max-width:1157px;
}
#container .section5 .map-info {
  flex: 1;
  background: #f5f5ff;
  padding: 40px 55px;
}
#container .section5 .map-info .ctit-wrap span {
  font-size: 16px;
  color: #999;
}
#container .section5 .map-info .ctit-wrap h2 {
