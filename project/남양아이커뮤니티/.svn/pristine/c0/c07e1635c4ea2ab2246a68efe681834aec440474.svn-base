.detail-text-box .detail-text {padding:27px 0 121px;font-size:15px;color:#444;line-height:25px;}
.detail-text-box .detail-cont {padding-bottom:30px;font-size:15px;color:#444;line-height:25px;}
.detail-text-box .detail-cont.type2 {padding-top:30px;}
.detail-text-box .detail-cont .point-txt1{display:block;font-size:18px;color:#ec1f2f;line-height:25px;font-weight:700;margin-bottom:10px;}
.detail-text-box .detail-cont .point-txt2{display:block;font-size:17px;color:#000;line-height:25px;font-weight:700;margin-bottom:10px;}
.detail-text-box .detail-cont .point-txt3{display:block;font-size:17px;color:#6e94f4;line-height:25px;font-weight:700;margin-top:30px;margin-bottom:10px;}
